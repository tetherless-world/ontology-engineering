<rdf:RDF xml:base="https://tw.rpi.edu/ontology-engineering/oe2020/beer-advisor/"
	xmlns:dct="http://purl.org/dc/terms/"
	xmlns:fibo-be-fct-fct="https://spec.edmcouncil.org/fibo/ontology/BE/FunctionalEntities/FunctionalEntities/"
	xmlns:fibo-fnd-aap-ppl="https://spec.edmcouncil.org/fibo/ontology/FND/AgentsAndPeople/People/"
	xmlns:fibo-fnd-arr-doc="https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Documents/"
	xmlns:fibo-fnd-arr-rt="https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Ratings/"
	xmlns:fibo-fnd-dt-fd="https://spec.edmcouncil.org/fibo/ontology/FND/DatesAndTimes/FinancialDates/"
	xmlns:fibo-fnd-pas-pas="https://spec.edmcouncil.org/fibo/ontology/FND/ProductsAndServices/ProductsAndServices/"
	xmlns:fibo-fnd-plc-loc="https://spec.edmcouncil.org/fibo/ontology/FND/Places/Locations/"
	xmlns:fibo-fnd-pty-pty="https://spec.edmcouncil.org/fibo/ontology/FND/Parties/Parties/"
	xmlns:fibo-fnd-qt-qtu="https://spec.edmcouncil.org/fibo/ontology/FND/Quantities/QuantitiesAndUnits/"
	xmlns:fibo-fnd-rel-rel="https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/"
	xmlns:fibo-fnd-utl-alx="https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/Analytics/"
	xmlns:fibo-fnd-utl-av="https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/"
	xmlns:lcc-cr="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/"
	xmlns:lcc-lr="https://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"
	xmlns:oe2020-beer="https://tw.rpi.edu/ontology-engineering/oe2020/beer-advisor/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:sm="http://www.omg.org/techprocess/ab/SpecificationMetadata/"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology rdf:about="https://tw.rpi.edu/ontology-engineering/oe2020/beer-advisor/">
		<rdfs:label>Beer Advisor Ontology</rdfs:label>
		<dct:abstract>This ontology ...</dct:abstract>
		<sm:dependsOn rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/ontology/BE/</sm:dependsOn>
		<sm:dependsOn rdf:datatype="&xsd;anyURI">https://spec.edmcouncil.org/fibo/ontology/FND/</sm:dependsOn>
		<sm:dependsOn rdf:datatype="&xsd;anyURI">https://www.omg.org/spec/LCC/</sm:dependsOn>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/BE/FunctionalEntities/FunctionalEntities/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/AgentsAndPeople/People/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Documents/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Arrangements/Ratings/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/DatesAndTimes/FinancialDates/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Parties/Parties/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Places/Locations/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/ProductsAndServices/ProductsAndServices/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Quantities/QuantitiesAndUnits/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/Analytics/"/>
		<owl:imports rdf:resource="https://spec.edmcouncil.org/fibo/ontology/FND/Utilities/AnnotationVocabulary/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/LCC/Countries/CountryRepresentation/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/LCC/Languages/LanguageRepresentation/"/>
		<owl:versionIRI rdf:resource="https://tw.rpi.edu/ontology-engineering/oe2020/20201025/beer-advisor/"/>
		<skos:changeNote>The https://tw.rpi.edu/ontology-engineering/oe2020/20201003/beer-advisor.rdf version of this ontology is the seed version.</skos:changeNote>
		<skos:changeNote>The https://tw.rpi.edu/ontology-engineering/oe2020/20201003/beer-advisor.rdf was revised to eliminate duplicate subclass of Product, add hasName to Brewery, and add the quantities ontology as an import.</skos:changeNote>
		<skos:changeNote>The https://tw.rpi.edu/ontology-engineering/oe2020/20201022/beer-advisor.rdf was revised to move named individuals into a separate ontology and corrected some confusion with units vs. quantities.</skos:changeNote>
	</owl:Ontology>
	
	<owl:Class rdf:about="&oe2020-beer;Alcohol">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;Characteristic"/>
		<rdfs:label>alcohol</rdfs:label>
		<skos:definition>sometimes referred to by the chemical name ethanol, is the psychoactive ingredient in beer</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://en.wikipedia.org/wiki/Alcohol_(drug)</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;AlcoholByVolume">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-qt-qtu;MeasurementUnit"/>
		<rdfs:label>alcohol by volume</rdfs:label>
		<skos:definition>proportion of the total volume of liquid that is alcohol</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom>J. M. Bernstein, The Complete Beer Course: Boot Camp for Beer Geeks: From Novice to Expert in Twelve Tasting Classes, Sterling Epicure; Illustrated Edition, 2013.</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;AlcoholContent">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-qt-qtu;QuantityValue"/>
		<rdfs:subClassOf rdf:resource="&oe2020-beer;Characteristic"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-qt-qtu;hasMeasurementUnit"/>
				<owl:onClass rdf:resource="&oe2020-beer;AlcoholByVolume"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>alcohol content</rdfs:label>
		<skos:definition>the amount of alcohol a liquid contains</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://en.wikipedia.org/wiki/Alcohol_by_volume</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;Ale">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;Beer"/>
		<rdfs:label>ale</rdfs:label>
		<skos:definition>type of beer brewed using warm fermentation method</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://en.wikipedia.org/wiki/Ale</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;AleYeast">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;Yeast"/>
		<rdfs:label>ale yeast</rdfs:label>
		<skos:definition>yeasts that favor high temperatures and that form a foam at the top of the wort during fermentation</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://en.wikipedia.org/wiki/Yeast</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;AmericanIndiaPaleAle">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;IndiaPaleAle"/>
		<rdfs:label>american india pale ale</rdfs:label>
		<skos:definition>ipa focusing more upon aroma and bitterness</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.craftbeer.com/beer/beer-styles-guide</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;Bar">
		<rdfs:subClassOf rdf:resource="&fibo-be-fct-fct;FunctionalBusinessEntity"/>
		<rdfs:label>bar</rdfs:label>
		<skos:definition>establishment where drinks, especially alcoholic drinks, are sold and consumed</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://dictionary.cambridge.org/dictionary/english/bar</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;Barley">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;Grain"/>
		<rdfs:label>barley</rdfs:label>
		<skos:definition>major cereal grain grown globally and used as a fermented source for beer</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://en.wikipedia.org/wiki/Barley</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;BaseMalt">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;Malt"/>
		<rdfs:label>base malt</rdfs:label>
		<skos:definition>light-colored malts that compose the bulk of the grains used in brewing process</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom>J. M. Bernstein, The Complete Beer Course: Boot Camp for Beer Geeks: From Novice to Expert in Twelve Tasting Classes, Sterling Epicure; Illustrated Edition, 2013.</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;Beer">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;Libation"/>
		<rdfs:subClassOf>
			<owl:Restriciton>
				<owl:onProperty rdf:resource="&oe2020-beer;hasIngredient"/>
				<owl:someValuesFrom rdf:resource="&oe2020-beer;Ingredient"/>
			</owl:Restriciton>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;isProducedBy"/>
				<owl:someValuesFrom rdf:resource="&oe2020-beer;Brewery"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&oe2020-beer;hasAlcoholByVolume"/>
				<owl:someValuesFrom rdf:resource="&oe2020-beer;AlcoholContent"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&oe2020-beer;hasBitternessUnit"/>
				<owl:someValuesFrom rdf:resource="&oe2020-beer;BitternessUnit"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&oe2020-beer;hasCharacteristic"/>
				<owl:someValuesFrom rdf:resource="&oe2020-beer;Characteristic"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&oe2020-beer;hasColor"/>
				<owl:someValuesFrom rdf:resource="&oe2020-beer;Color"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&oe2020-beer;hasOriginalGravity"/>
				<owl:someValuesFrom rdf:resource="&oe2020-beer;OriginalGravity"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>beer</rdfs:label>
		<skos:definition>beverage, often alcoholic, obtained from the fermentation of cereal grains</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom>T. Burnham, J. Herz, J. Holl, C. Jones, M. Storey, L. Trautwein, A. Sparhawk, G. Puza, N. Zander, &apos;Beer Styles Study Guide&apos; CraftBeer.com, 2020. [Online]. Available: https://www.craftbeer.com/beer/beer-styles-guide. [Accessed Sept. 23, 2020]</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.oxfordlearnersdictionaries.com/us/definition/english/beer?q=beer</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;Beverage">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-pas-pas;Product"/>
		<rdfs:label>beverage</rdfs:label>
		<skos:definition>consumable liquid</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.merriam-webster.com/dictionary/beverage</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;BitternessUnit">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-qt-qtu;QuantityValue"/>
		<rdfs:subClassOf rdf:resource="&oe2020-beer;Characteristic"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-qt-qtu;hasMeasurementUnit"/>
				<owl:onClass rdf:resource="&oe2020-beer;InternationalBitternessUnit"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>bitterness unit</rdfs:label>
		<skos:definition>unit of the measurement scale for the bitterness of beers</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://en.wikipedia.org/wiki/Beer_measurement#Bitterness/fibo-fnd-utl-av:adaptedFrom</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;Bock">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;Lager"/>
		<rdfs:label>bock</rdfs:label>
		<skos:definition>type of dark lager with strong malty flavor</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.craftbeer.com/beer/beer-styles-guide</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;Brand">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;Characteristic"/>
		<rdfs:label>brand</rdfs:label>
		<skos:definition>a public image, reputation, or identity conceived of as something to be marketed or promoted</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.merriam-webster.com/dictionary/brand</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;Brewery">
		<rdfs:subClassOf rdf:resource="&fibo-be-fct-fct;FunctionalBusinessEntity"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&lcc-lr;hasName"/>
				<owl:onDataRange rdf:resource="&xsd;string"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;produces"/>
				<owl:someValuesFrom rdf:resource="&oe2020-beer;Beer"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>brewery</rdfs:label>
		<skos:definition>place where beer is produced</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;Brewpub">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;Bar"/>
		<rdfs:subClassOf rdf:resource="&oe2020-beer;Brewery"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-pas-pas;sells"/>
				<owl:someValuesFrom rdf:resource="&oe2020-beer;Beer"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label xml:lang="en">brewpub</rdfs:label>
		<skos:definition>establishment that sells beverages brewed on the premises and often includes a restaurant</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.dictionary.com/browse/brewpub</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.merriam-webster.com/dictionary/brewpub</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;CaramelMalt">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;SpecialtyMalt"/>
		<rdfs:label>caramel malt</rdfs:label>
		<owl:equivalentClass rdf:resource="&oe2020-beer;CrystalMalt"/>
		<skos:definition>malts that are specially stewed to create crystalline sugar structures within the grain&apos;s hull</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom>J. M. Bernstein, The Complete Beer Course: Boot Camp for Beer Geeks: From Novice to Expert in Twelve Tasting Classes, Sterling Epicure; Illustrated Edition, 2013.</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;Characteristic">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-utl-alx;Aspect"/>
		<rdfs:label>characteristic</rdfs:label>
		<skos:definition>a typical set of features or qualities that a beer has</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.oxfordlearnersdictionaries.com/definition/english/characteristic_1?q=characteristic</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;Color">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-qt-qtu;MeasurementUnit"/>
		<rdfs:label>color</rdfs:label>
		<skos:definition>the appearance that beers have that results from the way in which they reflect light</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.oxfordlearnersdictionaries.com/definition/english/color_1?q=color</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;Corn">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;Grain"/>
		<rdfs:label>corn</rdfs:label>
		<skos:definition>cereal grains from species Zea mays that are used to lighten a beer&apos;s body, decrease haziness and stabilize flavor</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://en.wikipedia.org/wiki/Maize</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;CrystalMalt">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;SpecialtyMalt"/>
		<rdfs:label>crystal malt</rdfs:label>
		<skos:definition>malts that are specially stewed to create crystalline sugar structures within the grain&apos;s hull</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom>J. M. Bernstein, The Complete Beer Course: Boot Camp for Beer Geeks: From Novice to Expert in Twelve Tasting Classes, Sterling Epicure; Illustrated Edition, 2013.</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;DarkLager">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;Lager"/>
		<rdfs:label>dark lager</rdfs:label>
		<skos:definition>style of lager; pairs clean lager yeast with dark malts</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.craftbeer.com/beer/beer-styles-guide</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;DarkMalt">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;SpecialtyMalt"/>
		<rdfs:label>dark malt</rdfs:label>
		<skos:definition>malts that are highly roasted to add a very characteristic flavor</skos:definition>
