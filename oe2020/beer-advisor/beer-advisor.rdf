	<owl:Class rdf:about="&oe2020-beer;User">
		<rdfs:label>user</rdfs:label>
		<skos:definition>person who uses computer, application, or device</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.dictionary.com/browse/user</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;SearchHistory">
		<rdfs:label>search history</rdfs:label>
		<skos:definition>record of information browsed by user on computer</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.lexico.com/en/definition/browsing_history</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;Bar">
		<rdfs:subClassOf rdf:resource="&fibo-be-fct-fct;FunctionalBusinessEntity"/>
		<rdfs:label>bar</rdfs:label>
		<skos:definition>establishment where drinks, especially alcoholic drinks, are sold and consumed</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://dictionary.cambridge.org/dictionary/english/bar</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;Beer">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;Libation"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&fibo-fnd-rel-rel;isProducedBy"/>
				<owl:someValuesFrom rdf:resource="&oe2020-beer;Brewery"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>beer</rdfs:label>
		<skos:definition>beverage, often alcoholic, obtained from the fermentation of cereal grains</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom>T. Burnham, J. Herz, J. Holl, C. Jones, M. Storey, L. Trautwein, A. Sparhawk, G. Puza, N. Zander, &apos;Beer Styles Study Guide&apos; CraftBeer.com, 2020. [Online]. Available: https://www.craftbeer.com/beer/beer-styles-guide. [Accessed Sept. 23, 2020]</fibo-fnd-utl-av:adaptedFrom>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.oxfordlearnersdictionaries.com/us/definition/english/beer?q=beer</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;Beverage">
		<rdfs:subClassOf rdf:resource="&fibo-fnd-pas-pas;Product"/>
		<rdfs:label>beverage</rdfs:label>
		<skos:definition>consumable liquid</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.merriam-webster.com/dictionary/beverage</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;Ale">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;Beer"/>
		<rdfs:label>ale</rdfs:label>
		<skos:definition>type of beer brewed using warm fermentation method</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://en.wikipedia.org/wiki/Ale</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;Lager">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;Beer"/>
		<rdfs:label>lager</rdfs:label>
		<skos:definition>a type of beer conditioned at low temperature</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://en.wikipedia.org/wiki/Lager</fibo-fnd-utl-av:adaptedFrom> 
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;PaleAle">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;Ale"/>
		<rdfs:label>pale ale</rdfs:label>
		<skos:definition>type of ale; usually medium-colored, dry with a strong flavor of hops</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.merriam-webster.com/dictionary/pale%20ale</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;IndiaPaleAle">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;PaleAle"/>
		<rdfs:label>india pale ale</rdfs:label>
		<skos:definition>pale ale made with extra hops</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.merriam-webster.com/dictionary/India%20pale%20ale</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;EnglishIndiaPaleAle">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;IndiaPaleAle"/>
		<rdfs:label>english india pale ale</rdfs:label>
		<skos:definition>stronger version of pale ale, characterized by english hop and increased alcohol content</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.craftbeer.com/styles/english-style-india-pale-ale</fibo-fnd-utl-av:adaptedFrom>

	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;AmericanIndiaPaleAle">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;IndiaPaleAle"/>
		<rdfs:label>american india pale ale</rdfs:label>
		<skos:definition>ipa focusing more upon aroma and bitterness</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.craftbeer.com/beer/beer-styles-guide</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;NewEnglandIndiaPaleAle">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;IndiaPaleAle"/>
		<rdfs:label>new england india pale ale</rdfs:label>
		<skos:definition>IPA with emphasis upon juicy and tropical hops, without emphasis on bitter</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.craftbeer.com/beer/beer-styles-guide</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;ImperialIndiaPaleAle">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;IndiaPaleAle"/>
		<rdfs:label>imperial india pale ale</rdfs:label>
		<skos:definition>pale ale made with extra hops</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.merriam-webster.com/dictionary/India%20pale%20ale</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;SessionIndiaPaleAle">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;Hybrid"/>
		<rdfs:label>session india pale ale</rdfs:label>
		<skos:definition>combiines hoppiness of IPA with lower alcohol content of session beer</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://beerandwinejournal.com/session-ipa/#:~:text=One%20style%20(or%20substyle)%20of,one%20of%20the%20best%2Dknown.</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;DarkLager">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;Lager"/>
		<rdfs:label>dark lager</rdfs:label>
		<skos:definition>style of lager; pairs clean lager yeast with dark malts</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.craftbeer.com/beer/beer-styles-guide</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;IrishDryStout">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;Stout"/>
		<rdfs:label>irish dry stout</rdfs:label>
		<skos:definition>black beer with dry-roasted character due to roasted barley</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.craftbeer.com/styles/irish-style-dry-stout#:~:text=Dry%20stout%20is%20black%20beer,is%20medium%20to%20medium%20high.</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;OatmealStout">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;Stout"/>
		<rdfs:label>oatmeal stout</rdfs:label>
		<skos:definition>dark brown to black beer with roaste dmalt character and smooth, rich body due to oatmeal</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.beeradvocate.com/beer/styles/69/</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;Bock">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;Lager"/>
		<rdfs:label>bock</rdfs:label>
		<skos:definition>beer that is a combination of the lager and ale style</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.thespruceeats.com/what-are-hybrid-beers-353065</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe2020-beer;Hybrid">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;Beer"/>
		<rdfs:label>hybrid</rdfs:label>
		<skos:definition>beer that is a combination of the lager and ale style</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.thespruceeats.com/what-are-hybrid-beers-353065</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>
	
