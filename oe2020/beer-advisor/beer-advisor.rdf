	<owl:ObjectProperty rdf:about="&oe2020-beer;hasCharacteristic">
		<rdfs:label></rdfs:label>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&oe2020-beer;hasIntrinsicCharacteristic">
		<rdfs:label></rdfs:label>
		<rdfs:subPropertyOf rdf:resource="&oe2020-beer;hasCharacteristic"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&oe2020-beer;hasExtrensicCharacteristic">
		<rdfs:label></rdfs:label>
		<rdfs:subPropertyOf rdf:resource="&oe2020-beer;hasCharacteristic"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&oe2020-beer;hasAlcoholByVolume">
		<rdfs:label></rdfs:label>
		<rdfs:subPropertyOf rdf:resource="&oe2020-beer;hasIntrinsicCharacteristic"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&oe2020-beer;hasOriginalGravity">
