		<skos:definition>one of the things from which beer is made</skos:definition>
		<fibo-fnd-utl-av:adaptedFrom rdf:datatype="&xsd;anyURI">https://www.oxfordlearnersdictionaries.com/definition/english/ingredient?q=ingredient</fibo-fnd-utl-av:adaptedFrom>
	</owl:Class>

	<owl:Class rdf:about="&oe2020-beer;Hop">
		<rdfs:subClassOf rdf:resource="&oe2020-beer;Ingredient"/>
