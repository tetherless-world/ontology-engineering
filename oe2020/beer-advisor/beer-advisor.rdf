		<rdfs:subPropertyOf rdf:resource="&oe2020-beer;hasIntrinsicCharacteristic"/>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&oe2020-beer;hasInternationBitternessUnit">
