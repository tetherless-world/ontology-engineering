<!DOCTYPE rdf:RDF [
	<!ENTITY cmns-av "https://www.omg.org/spec/Commons/AnnotationVocabulary/">
	<!ENTITY cmns-doc "https://www.omg.org/spec/Commons/Documents/">
	<!ENTITY cmns-dt "https://www.omg.org/spec/Commons/DatesAndTimes/">
	<!ENTITY cmns-org "https://www.omg.org/spec/Commons/Organizations/">
	<!ENTITY cmns-pts "https://www.omg.org/spec/Commons/PartiesAndSituations/">
	<!ENTITY cmns-rlcmp "https://www.omg.org/spec/Commons/RolesAndCompositions/">
	<!ENTITY dtc "http://purl.org/dc/terms/">
	<!ENTITY oe-wtgw "https://tw.rpi.edu/ontology-engineering/oe2024/when-to-go-where/WhenToGoWhere/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF xml:base="https://tw.rpi.edu/ontology-engineering/oe2024/when-to-go-where/WhenToGoWhere/"
	xmlns="https://tw.rpi.edu/ontology-engineering/oe2024/when-to-go-where/WhenToGoWhere/"
	xmlns:cmns-av="https://www.omg.org/spec/Commons/AnnotationVocabulary/"
	xmlns:cmns-doc="https://www.omg.org/spec/Commons/Documents/"
	xmlns:cmns-dt="https://www.omg.org/spec/Commons/DatesAndTimes/"
	xmlns:cmns-org="https://www.omg.org/spec/Commons/Organizations/"
	xmlns:cmns-pts="https://www.omg.org/spec/Commons/PartiesAndSituations/"
	xmlns:cmns-rlcmp="https://www.omg.org/spec/Commons/RolesAndCompositions/"
	xmlns:dtc="http://purl.org/dc/terms/"
	xmlns:oe-wtgw="https://tw.rpi.edu/ontology-engineering/oe2024/when-to-go-where/WhenToGoWhere/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology rdf:about="https://tw.rpi.edu/ontology-engineering/oe2024/when-to-go-where/WhenToGoWhere/">
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/AnnotationVocabulary/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/DatesAndTimes/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/Documents/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/Organizations/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/PartiesAndSituations/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/RolesAndCompositions/"/>
	</owl:Ontology>
	
	<owl:Class rdf:about="&oe-wtgw;Accomodations">
		<rdfs:label>Accomodations</rdfs:label>
		<skos:definition>Temporary lodgings, sometimes also including board.</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;Activities">
		<rdfs:label>Activities</rdfs:label>
		<skos:definition>A use of the park by visitors</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;Amphibian">
		<rdfs:subClassOf rdf:resource="&oe-wtgw;Animal"/>
		<rdfs:label>Amphibian</rdfs:label>
		<skos:definition>A cold-blooded vertebrate animal that typically starts life in water with gills and later develops lungs for life on land, such as frogs and salamanders.</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;Amphitheatre">
		<rdfs:subClassOf rdf:resource="&oe-wtgw;Facilities"/>
		<rdfs:label>Amphitheatre</rdfs:label>
		<skos:definition>A circular or oval open-air venue with seating around a central performance area, often used for events in parks or natural settings.</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;Animal">
		<rdfs:label>Animal</rdfs:label>
		<skos:definition>A living organism that feeds on organic matter, typically having specialized sense organs and nervous systems and able to move voluntarily.</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;AutoCaravan">
		<rdfs:subClassOf rdf:resource="&oe-wtgw;Activities"/>
		<rdfs:label>AutoCaravan</rdfs:label>
		<skos:definition>A motor vehicle equipped for living in, typically used for recreational purposes, often referred to as a camper van or motorhome.</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;BaldEagle">
		<rdfs:subClassOf rdf:resource="&oe-wtgw;Bird"/>
		<rdfs:label>BaldEagle</rdfs:label>
		<skos:definition>Subclass of the Bird Class that describes a Bald Eagle</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;Bird">
		<rdfs:subClassOf rdf:resource="&oe-wtgw;Animal"/>
		<rdfs:label>Birds</rdfs:label>
		<skos:definition>A warm-blooded vertebrate animal characterized by feathers, beaks, and typically the ability to fly, with most species laying eggs.</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;BlackBear">
		<rdfs:subClassOf rdf:resource="&oe-wtgw;Mammal"/>
		<rdfs:label>BlackBear</rdfs:label>
		<skos:definition>Subclass of Mammal Class that describes a Black Bear</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;CampSite">
		<rdfs:subClassOf rdf:resource="&oe-wtgw;Accomodations"/>
		<rdfs:label>CampSite</rdfs:label>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;Caribou">
		<rdfs:subClassOf rdf:resource="&oe-wtgw;Mammal"/>
		<rdfs:label>Caribou</rdfs:label>
		<skos:definition>Subclass of Mammal Class that describes a Caribou</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;Climate">
		<rdfs:label>Climate</rdfs:label>
		<skos:definition>The average weather conditions in a particular region over a long period, including temperature, humidity, wind patterns, and precipitation.</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;ComfortStation">
		<rdfs:subClassOf rdf:resource="&oe-wtgw;Facilities"/>
		<rdfs:label>ComfortStation</rdfs:label>
		<skos:definition>A facility with restrooms, sinks, and sometimes showers, provided for public use, especially in camping or recreational areas.</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;Coyote">
		<rdfs:subClassOf rdf:resource="&oe-wtgw;Mammal"/>
		<rdfs:label>Coyote</rdfs:label>
		<skos:definition>Subclass of Mammal Class that describes a Coyote</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;Facilities">
		<rdfs:label>Facilities</rdfs:label>
		<skos:definition>Improvements and other material things provided for the use, accommodation, and convenience of park visitors</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;GrayWolf">
		<rdfs:subClassOf rdf:resource="&oe-wtgw;Mammal"/>
		<rdfs:label>Gray Wolf</rdfs:label>
		<skos:definition>Subclass of Mammal Class that describes a Gray Wolf</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;Hike">
		<rdfs:subClassOf rdf:resource="&oe-wtgw;Activities"/>
		<rdfs:label>Hike</rdfs:label>
		<skos:definition>A long walk, typically through countryside or wilderness areas, undertaken for leisure or exercise.</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;InformationStation">
		<rdfs:subClassOf rdf:resource="&oe-wtgw;Facilities"/>
		<rdfs:label>InformationStation</rdfs:label>
		<skos:definition>A booth or building where visitors can receive assistance, guides, and information regarding their surroundings in parks, campsites, or other public areas.</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;Insect">
		<rdfs:subClassOf rdf:resource="&oe-wtgw;Animal"/>
		<rdfs:label>Insect</rdfs:label>
		<skos:definition>A small arthropod animal with a segmented body, six legs, and typically wings, such as flies, bees, and ants.</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;License">
		<rdfs:subClassOf rdf:resource="&cmns-doc;LegalDocument"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-pts;holdsDuring"/>
				<owl:onClass rdf:resource="&cmns-dt;DatePeriod"/>
				<owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&oe-wtgw;isIssuedBy"/>
				<owl:someValuesFrom rdf:resource="&oe-wtgw;Licensor"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-pts;hasPartyRole"/>
				<owl:someValuesFrom rdf:resource="&oe-wtgw;Licensee"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>license</rdfs:label>
		<skos:definition>grant of permission needed to do something</skos:definition>
		<cmns-av:adaptedFrom>Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</cmns-av:adaptedFrom>
		<cmns-av:explanatoryNote>Note that in some cases, a license may also be considered an agreement or contract, depending on the specifics of the license and jurisdiction.</cmns-av:explanatoryNote>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;Licensee">
		<rdfs:subClassOf rdf:resource="&cmns-pts;PartyRole"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-rlcmp;isPlayedBy"/>
				<owl:someValuesFrom>
					<owl:Restriction>
						<owl:onProperty rdf:resource="&cmns-pts;isAPartyTo"/>
						<owl:someValuesFrom rdf:resource="&oe-wtgw;License"/>
					</owl:Restriction>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>licensee</rdfs:label>
		<skos:definition>a party to whom a license has been granted</skos:definition>
		<cmns-av:adaptedFrom>Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</cmns-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;Licensor">
		<rdfs:subClassOf rdf:resource="&cmns-pts;PartyRole"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&oe-wtgw;issues"/>
				<owl:someValuesFrom rdf:resource="&oe-wtgw;License"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&oe-wtgw;licenses"/>
				<owl:someValuesFrom rdf:resource="&owl;Thing"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-rlcmp;isPlayedBy"/>
				<owl:someValuesFrom>
					<owl:Restriction>
						<owl:onProperty rdf:resource="&cmns-pts;isAPartyTo"/>
						<owl:someValuesFrom rdf:resource="&oe-wtgw;License"/>
					</owl:Restriction>
				</owl:someValuesFrom>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>licensor</rdfs:label>
		<skos:definition>a party who grants a license</skos:definition>
		<cmns-av:adaptedFrom>Barron&apos;s Dictionary of Business and Economics Terms, Fifth Edition, 2012</cmns-av:adaptedFrom>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;LoopTrail">
		<rdfs:subClassOf rdf:resource="&oe-wtgw;Hike"/>
		<rdfs:label>LoopTrail</rdfs:label>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;Mammal">
		<rdfs:subClassOf rdf:resource="&oe-wtgw;Animal"/>
		<rdfs:label>Mammals</rdfs:label>
		<skos:definition>A warm-blooded vertebrate animal of a class that is distinguished by the presence of hair or fur, and females typically have mammary glands that produce milk for their young.</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;MountainGoat">
		<rdfs:subClassOf rdf:resource="&oe-wtgw;Mammal"/>
		<rdfs:label>MountainGoat</rdfs:label>
		<skos:definition>Subclass of Mammal Class that describes a Mountain Goat</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;MountainLion">
		<rdfs:subClassOf rdf:resource="&oe-wtgw;Mammal"/>
		<rdfs:label>MountainLion</rdfs:label>
		<skos:definition>Subclass of Mammal Class that describes a Mountain Lion</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;MuleDeer">
		<rdfs:subClassOf rdf:resource="&oe-wtgw;Mammal"/>
		<rdfs:label>MuleDeer</rdfs:label>
		<skos:definition>Subclass of Mammal Class that describes a Mule Deer</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;NationalPark">
		<rdfs:label>NationalPark</rdfs:label>
		<skos:definition>A large area of land or water that is protected by a government for the preservation of its natural environment, history, or culture</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;NatureWalk">
		<rdfs:subClassOf rdf:resource="&oe-wtgw;Activities"/>
		<rdfs:label>NatureWalk</rdfs:label>
		<skos:definition>a walk on a nature trail, especially with an experienced guide</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;OutandBackTrail">
		<rdfs:subClassOf rdf:resource="&oe-wtgw;Hike"/>
		<rdfs:label>OutandBackTrail</rdfs:label>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;ParkStatistic">
		<rdfs:label>park statistic</rdfs:label>
		<skos:definition>statistics about a park, including visitation numbers, trail usage, wildlife sightings, and other relevant data to monitor park activity and health</skos:definition>
	</owl:Class>

	<owl:Class rdf:about="&oe-wtgw;Parking">
		<rdfs:subClassOf rdf:resource="&oe-wtgw;Facilities"/>
		<rdfs:label>Parking</rdfs:label>
		<skos:definition>A space or lot where vehicles can be parked, typically found at entrances to parks, trails, or campsites.</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;Reptile">
		<rdfs:subClassOf rdf:resource="&oe-wtgw;Animal"/>
		<rdfs:label>Reptile</rdfs:label>
		<skos:definition>A cold-blooded vertebrate animal that has dry, scaly skin and typically lays soft-shelled eggs on land, such as snakes, lizards, and turtles.</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;Terrain">
		<rdfs:label>Terrain</rdfs:label>
		<skos:definition>The physical features of a piece of land, including its elevation, slope, and orientation, often used to describe hiking or camping environments.</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;TrailHead">
		<rdfs:subClassOf rdf:resource="&oe-wtgw;Hike"/>
		<rdfs:label>TrailHead</rdfs:label>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-wtgw;WesternRattlesnake">
		<rdfs:subClassOf rdf:resource="&oe-wtgw;Reptile"/>
		<rdfs:label>WesternRattlesnake</rdfs:label>
		<skos:definition>Subclass of Reptile Class that describes a Western Rattlesnake</skos:definition>
	</owl:Class>
	
	<owl:ObjectProperty rdf:about="&oe-wtgw;isIssuedBy">
		<rdfs:subPropertyOf rdf:resource="&cmns-org;isProvidedBy"/>
		<rdfs:label>is issued by</rdfs:label>
		<rdfs:range rdf:resource="&cmns-pts;AgentRole"/>
		<owl:inverseOf rdf:resource="&oe-wtgw;issues"/>
		<skos:definition>indicates a role played by a functional entity or party responsible for circulating, distributing, or publishing something</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&oe-wtgw;issues">
		<rdfs:subPropertyOf rdf:resource="&cmns-org;provides"/>
		<rdfs:label>issues</rdfs:label>
		<rdfs:domain rdf:resource="&cmns-pts;AgentRole"/>
		<skos:definition>officially makes something available</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&oe-wtgw;licenses">
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&cmns-org;isProvidedBy">
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&cmns-org;provides">
	</owl:ObjectProperty>

</rdf:RDF>