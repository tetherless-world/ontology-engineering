<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
	<!ENTITY cmns-av "https://www.omg.org/spec/Commons/AnnotationVocabulary/">
	<!ENTITY cmns-cls "https://www.omg.org/spec/Commons/Classifiers/">
	<!ENTITY cmns-col "https://www.omg.org/spec/Commons/Collections/">
	<!ENTITY cmns-doc "https://www.omg.org/spec/Commons/Documents/">
	<!ENTITY cmns-dsg "https://www.omg.org/spec/Commons/Designators/">
	<!ENTITY cmns-dt "https://www.omg.org/spec/Commons/DatesAndTimes/">
	<!ENTITY cmns-org "https://www.omg.org/spec/Commons/Organizations/">
	<!ENTITY cmns-pts "https://www.omg.org/spec/Commons/PartiesAndSituations/">
	<!ENTITY cmns-qtu "https://www.omg.org/spec/Commons/QuantitiesAndUnits/">
	<!ENTITY cmns-rlcmp "https://www.omg.org/spec/Commons/RolesAndCompositions/">
	<!ENTITY cmns-txt "https://www.omg.org/spec/Commons/TextDatatype/">
	<!ENTITY dtc "http://purl.org/dc/terms/">
	<!ENTITY oe-pdt "https://tw.rpi.edu/ontology-engineering/oe2024/personalized-depression-treatment/PersonalizedDepressionTreatment/">
	<!ENTITY owl "http://www.w3.org/2002/07/owl#">
	<!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
	<!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
	<!ENTITY skos "http://www.w3.org/2004/02/skos/core#">
	<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF xml:base="https://tw.rpi.edu/ontology-engineering/oe2024/personalized-depression-treatment/PersonalizedDepressionTreatment/"
	xmlns="https://tw.rpi.edu/ontology-engineering/oe2024/personalized-depression-treatment/PersonalizedDepressionTreatment/"
	xmlns:cmns-av="https://www.omg.org/spec/Commons/AnnotationVocabulary/"
	xmlns:cmns-cls="https://www.omg.org/spec/Commons/Classifiers/"
	xmlns:cmns-col="https://www.omg.org/spec/Commons/Collections/"
	xmlns:cmns-doc="https://www.omg.org/spec/Commons/Documents/"
	xmlns:cmns-dsg="https://www.omg.org/spec/Commons/Designators/"
	xmlns:cmns-dt="https://www.omg.org/spec/Commons/DatesAndTimes/"
	xmlns:cmns-org="https://www.omg.org/spec/Commons/Organizations/"
	xmlns:cmns-pts="https://www.omg.org/spec/Commons/PartiesAndSituations/"
	xmlns:cmns-qtu="https://www.omg.org/spec/Commons/QuantitiesAndUnits/"
	xmlns:cmns-rlcmp="https://www.omg.org/spec/Commons/RolesAndCompositions/"
	xmlns:cmns-txt="https://www.omg.org/spec/Commons/TextDatatype/"
	xmlns:dtc="http://purl.org/dc/terms/"
	xmlns:oe-pdt="https://tw.rpi.edu/ontology-engineering/oe2024/personalized-depression-treatment/PersonalizedDepressionTreatment/"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
	
	<owl:Ontology rdf:about="https://tw.rpi.edu/ontology-engineering/oe2024/personalized-depression-treatment/PersonalizedDepressionTreatment/">
		<rdfs:label>personalized depression treatment ontology</rdfs:label>
		<dtc:license>placeholder</dtc:license>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/AnnotationVocabulary/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/Classifiers/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/Collections/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/DatesAndTimes/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/Designators/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/Documents/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/Organizations/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/PartiesAndSituations/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/QuantitiesAndUnits/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/RolesAndCompositions/"/>
		<owl:imports rdf:resource="https://www.omg.org/spec/Commons/TextDatatype/"/>
		<cmns-av:copyright>placeholder</cmns-av:copyright>
	</owl:Ontology>
	
	<owl:Class rdf:about="&oe-pdt;Anhedonia">
		<rdfs:subClassOf rdf:resource="&oe-pdt;Symptom"/>
		<rdfs:label>anhedonia</rdfs:label>
		<skos:definition>A core symptom of depression involving the inability to experience pleasure in normally enjoyable activities.</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;Antidepressant">
		<rdfs:subClassOf rdf:resource="&oe-pdt;DrugTreatment"/>
		<rdfs:label>antidepressant</rdfs:label>
		<skos:definition>prescription medication used to treat depression or other mental health conditions</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;AnxietyDisorder">
		<rdfs:subClassOf rdf:resource="&oe-pdt;PsychiatricDisorder"/>
		<rdfs:label>anxiety disorder</rdfs:label>
		<skos:definition>A group of disorders characterized by a persistent anxiety that can can interfere with daily activities such as job performance, schoolwork, and relationships</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;BipolarDisorder">
		<rdfs:subClassOf rdf:resource="&oe-pdt;PsychiatricDisorder"/>
		<rdfs:label>bipolar disorder</rdfs:label>
		<skos:definition>A mental illness that causes unusual shifts in mood, energy, activity levels, and concentration. These shifts can make it difficult to carry out daily tasks.</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;ChronicPain">
		<rdfs:subClassOf rdf:resource="&oe-pdt;Symptom"/>
		<rdfs:label>chronic pain</rdfs:label>
		<skos:definition>Pain that lasts for over three months.</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;CoexpressionModule">
		<rdfs:subClassOf rdf:resource="&cmns-rlcmp;Composition"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-col;hasConstituent"/>
				<owl:someValuesFrom rdf:resource="&oe-pdt;Gene"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>coexpression module</rdfs:label>
		<skos:definition>group of genes that serve a similar purpose</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;CognitiveBehavioralTherapy">
		<rdfs:subClassOf rdf:resource="&oe-pdt;TherapeuticTreatment"/>
		<rdfs:label>cognitive behavioral therapy</rdfs:label>
		<skos:altLabel>CBT</skos:altLabel>
		<skos:definition>psychotherapy focused on modifying negative patterns of thought and behavior</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;Depression">
		<rdfs:subClassOf rdf:resource="&oe-pdt;Symptom"/>
		<rdfs:label>depression</rdfs:label>
		<skos:definition>A prolonged state of sadness, despair, or loneliness.</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;DepressionRatingScale">
		<rdfs:subClassOf rdf:resource="&cmns-qtu;MaterialMeasure"/>
		<rdfs:label>depression rating scale</rdfs:label>
		<skos:definition>test used to determine to what extent a patient has depression</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;Diagnosis">
		<rdfs:subClassOf rdf:resource="&cmns-pts;Situation"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&oe-pdt;diagnoses"/>
				<owl:onClass rdf:resource="&oe-pdt;PsychiatricDisorder"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>diagnosis</rdfs:label>
		<skos:definition>disorder with which a patient has been identified as having</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;DiseaseAssociatedGene">
		<rdfs:label>disease-associated gene</rdfs:label>
		<owl:equivalentClass>
			<owl:Class>
				<owl:intersectionOf rdf:parseType="Collection">
					<rdf:Description rdf:about="&oe-pdt;Gene">
					</rdf:Description>
					<owl:Restriction>
						<owl:onProperty rdf:resource="&oe-pdt;isAssociatedWith"/>
						<owl:onClass rdf:resource="&oe-pdt;PsychiatricDisorder"/>
						<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
					</owl:Restriction>
				</owl:intersectionOf>
			</owl:Class>
		</owl:equivalentClass>
		<skos:definition>An allele which is more prevalent than expected among populations with a given disease.</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;Dosage">
		<rdfs:subClassOf rdf:resource="&cmns-qtu;Variable"/>
		<rdfs:label>dosage</rdfs:label>
		<skos:definition>The amount of medicine that you should take at one time</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;DrugTreatment">
		<rdfs:subClassOf rdf:resource="&oe-pdt;Treatment"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&oe-pdt;hasBrandName"/>
				<owl:someValuesFrom rdf:resource="&cmns-dsg;Name"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-dsg;hasName"/>
				<owl:someValuesFrom rdf:resource="&cmns-dsg;Name"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>drug treatment</rdfs:label>
		<skos:definition>use of medications to manage, alleviate, or cure a medical condition or disease</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;Gene">
		<rdfs:subClassOf rdf:resource="&cmns-col;Constituent"/>
		<rdfs:label>gene</rdfs:label>
		<skos:definition>distinct sequence of nucleotides forming part of a chromosome, the order of which determines the order of monomers in a polypeptide or nucleic acid molecule which a cell (or virus) may synthesize</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;GeneExpression">
		<rdfs:subClassOf rdf:resource="&cmns-col;Constituent"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-col;isConstituentOf"/>
				<owl:allValuesFrom rdf:resource="&oe-pdt;Transcriptome"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>gene expression</rdfs:label>
		<skos:definition>specific sequence in the genome that can be associated with certain traits, including responses to medications</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;Genome">
		<rdfs:subClassOf rdf:resource="&cmns-rlcmp;Composition"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-col;hasConstituent"/>
				<owl:someValuesFrom rdf:resource="&oe-pdt;Gene"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>genome</rdfs:label>
		<skos:definition>entire set of DNA instructions found in a living organism</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;Ketamine">
		<rdfs:subClassOf rdf:resource="&oe-pdt;DrugTreatment"/>
		<rdfs:label>ketamine</rdfs:label>
		<skos:definition>dissociative anesthetic used medically for induction and maintenance of anesthesia; is also used as a treatment for depression and in pain management.</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;Lithium">
		<rdfs:subClassOf rdf:resource="&oe-pdt;DrugTreatment"/>
		<rdfs:label>lithium</rdfs:label>
		<skos:definition>metal carbonate used as a mood-stabilizing drug</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;MajorDepressiveDisorder">
		<rdfs:subClassOf rdf:resource="&oe-pdt;PsychiatricDisorder"/>
		<rdfs:label>major depressive disorder</rdfs:label>
		<skos:altLabel>MDD</skos:altLabel>
		<skos:definition>A mental health condition characterized by persistent and intense feelings of sadness and despair, lasting at least two weeks.</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;MedicalProfessional">
		<rdfs:subClassOf rdf:resource="&cmns-pts;PartyRole"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-rlcmp;isPlayedBy"/>
				<owl:someValuesFrom rdf:resource="&oe-pdt;Person"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>medical professional</rdfs:label>
		<skos:definition>someone who provides treatments to patients</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;MindfulnessBasedCognitiveTherapy">
		<rdfs:subClassOf rdf:resource="&oe-pdt;TherapeuticTreatment"/>
		<rdfs:label>mindfulness-based cognitive therapy</rdfs:label>
		<skos:altLabel>MBCT</skos:altLabel>
		<skos:definition>therapeutic treatment that combines mindfulness practices with elements of cognitive therapy to help individuals better understand and manage their thoughts and emotions to reduce mental distress</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;Neurotransmitter">
		<rdfs:subClassOf rdf:resource="&oe-pdt;substance"/>
		<rdfs:label>neurotransmitter</rdfs:label>
		<skos:definition>chemical messengers in the brain that transmit signals between nerve cells and play a key role in mood regulation</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;Norepinephrine-DopamineReuptakeInhibitor">
		<rdfs:subClassOf rdf:resource="&oe-pdt;ReuptakeInhibitor"/>
		<rdfs:label>norepinephrine-dopamine reuptake inhibitor</rdfs:label>
		<skos:altLabel>NDRI</skos:altLabel>
		<skos:definition>class of antidepressants that works by inhibiting the reuptake of norepinephrine and dopamine, helping with mood regulation</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;Patient">
		<rdfs:subClassOf rdf:resource="&cmns-pts;PartyRole"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&oe-pdt;hasSymptom"/>
				<owl:allValuesFrom rdf:resource="&oe-pdt;Symptom"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&oe-pdt;hasDiagnosis"/>
				<owl:onClass rdf:resource="&oe-pdt;Diagnosis"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&oe-pdt;hasGenome"/>
				<owl:onClass rdf:resource="&oe-pdt;Genome"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&oe-pdt;hasResponse"/>
				<owl:onClass rdf:resource="&oe-pdt;TreatmentResponse"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-rlcmp;isPlayedBy"/>
				<owl:someValuesFrom rdf:resource="&oe-pdt;Person"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>patient</rdfs:label>
		<skos:definition>role of a person receiving or registered to receive medical treatment</skos:definition>
		<skos:editorialNote>In the pattern in Commons, experiences is a property chain from an individual thing to a role that thing plays to another role that thing undergoes. Thus saying that patient (role) &apos;experiences&apos; (property chain whose range is a role) &apos;symptom&apos; (which is a characteristic, not a role in your current ontology) would be logically inconsistent (role and agent are disjoint). What you want here is &apos;patient (role) &apos;acts in&apos; some &apos;situation&apos; (e.g. &apos;person with a condition&apos;, as a subclass of situation) that is characterized by one or more symptoms. The restriction on diagnosis stating that it diagnoses a psychiatric disorder is also not consistent with the way its domain and range are modeled. Linking the characteristic symptoms of the disorder to the &apos;person with a condition&apos; situation would likely need a different property chain that would be local to your model to indicate which symptom(s) the patient actually has of those that are characteristics. As a short term fix, I&apos;ve replaced &apos;experiences&apos; with &apos;has symptom&apos;, and deleted the domain on has symptom, as that originally was limited to the disorder. I think patients have symptoms, and disorders are characterized by those symptoms, rather than the other way around.</skos:editorialNote>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;Person">
		<rdfs:subClassOf rdf:resource="&cmns-pts;Agent"/>
		<rdfs:label>person</rdfs:label>
		<skos:definition>an individual human</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;PsychiatricDisorder">
		<rdfs:subClassOf rdf:resource="&cmns-doc;Specification"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&oe-pdt;hasSymptom"/>
				<owl:onClass rdf:resource="&oe-pdt;Symptom"/>
				<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">0</owl:minQualifiedCardinality>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>psychiatric disorder</rdfs:label>
		<skos:definition>behavioral, emotional, or cognitive dysfunction that is not readily controlled by the individual and are related to clinically significant distress or impairment in one or more areas including social, occupational, and interpersonal functioning</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;ReuptakeInhibitor">
		<rdfs:label>reuptake inhibitor</rdfs:label>
		<owl:equivalentClass>
			<owl:Class>
				<owl:intersectionOf rdf:parseType="Collection">
					<rdf:Description rdf:about="&oe-pdt;Antidepressant">
					</rdf:Description>
					<owl:Restriction>
						<owl:onProperty rdf:resource="&oe-pdt;preventsReuptakeOf"/>
						<owl:someValuesFrom rdf:resource="&oe-pdt;Neurotransmitter"/>
					</owl:Restriction>
				</owl:intersectionOf>
			</owl:Class>
		</owl:equivalentClass>
		<skos:definition>substance that increases the levels of neurotransmitter in the brain by preventing their reabsorption by nerve cells</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;SelectiveSerotoninReuptakeInhibitor">
		<rdfs:subClassOf rdf:resource="&oe-pdt;ReuptakeInhibitor"/>
		<rdfs:label>selective serotonin reuptake inhibitor</rdfs:label>
		<skos:altLabel>SSRI</skos:altLabel>
		<skos:definition>class of antidepressants used to treat major depressive disorder by increasing levels of serotonin in the brain</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;SerotoninNorepinephrineReuptakeInhibitor">
		<rdfs:subClassOf rdf:resource="&oe-pdt;ReuptakeInhibitor"/>
		<rdfs:label>serotonin and norepinephrine reuptake inhibitor</rdfs:label>
		<skos:altLabel>SNRI</skos:altLabel>
		<skos:definition>class of antidepressants that work by inhibiting the reabsorption of both serotonin and norepinephrine increasing their concentration in the brain</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;Symptom">
		<rdfs:subClassOf rdf:resource="&cmns-cls;Aspect"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-dt;hasEndDate"/>
				<owl:allValuesFrom rdf:resource="&cmns-dt;Date"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-cls;characterizes"/>
				<owl:someValuesFrom rdf:resource="&oe-pdt;PsychiatricDisorder"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-dt;hasStartDate"/>
				<owl:someValuesFrom rdf:resource="&cmns-dt;Date"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>symptom</rdfs:label>
		<skos:definition>physical or mental feature which is regarded as indicating a condition of disease</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;TherapeuticTreatment">
		<rdfs:subClassOf rdf:resource="&oe-pdt;Treatment"/>
		<rdfs:label>thearpeutic treatment</rdfs:label>
		<skos:definition>medical, surgical, or behavioral intervention to manage, alleviate, or cure a medical condition or disease</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;Transcriptome">
		<rdfs:subClassOf rdf:resource="&cmns-rlcmp;Composition"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&cmns-col;hasConstituent"/>
				<owl:allValuesFrom rdf:resource="&oe-pdt;GeneExpression"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>transcriptome</rdfs:label>
		<skos:definition>full range of mRNA molecules expressed by an individual</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;Treatment">
		<rdfs:subClassOf rdf:resource="&cmns-rlcmp;FunctionalRole"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&oe-pdt;hasOnset"/>
				<owl:allValuesFrom rdf:resource="&cmns-dt;Duration"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>treatment</rdfs:label>
		<dtc:description>something that serves to mitigate the symptoms of a medical disorder</dtc:description>
		<skos:definition>something used to manage, alleviate, or cure a medical condition or disease</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;TreatmentResistantDepression">
		<rdfs:label>treatment resistant depression</rdfs:label>
		<owl:equivalentClass>
			<owl:Class>
				<owl:intersectionOf rdf:parseType="Collection">
					<rdf:Description rdf:about="&oe-pdt;MajorDepressiveDisorder">
					</rdf:Description>
					<owl:Restriction>
						<owl:onProperty rdf:resource="&oe-pdt;resistsTreatment"/>
						<owl:onClass rdf:resource="&oe-pdt;Treatment"/>
						<owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minQualifiedCardinality>
					</owl:Restriction>
				</owl:intersectionOf>
			</owl:Class>
		</owl:equivalentClass>
		<skos:altLabel>TRD</skos:altLabel>
		<skos:definition>A form of Major Depressive Disorder that has shown resistance to two or more treatments.</skos:definition>
	</owl:Class>
	
	<owl:Class rdf:about="&oe-pdt;TreatmentResponse">
		<rdfs:subClassOf rdf:resource="&cmns-doc;Specification"/>
		<rdfs:subClassOf>
			<owl:Restriction>
				<owl:onProperty rdf:resource="&oe-pdt;isResponseTo"/>
				<owl:someValuesFrom rdf:resource="&oe-pdt;Treatment"/>
			</owl:Restriction>
		</rdfs:subClassOf>
		<rdfs:label>treatment response</rdfs:label>
		<skos:definition>degree to which symptoms improve as a result of a particular treatment.</skos:definition>
	</owl:Class>
	
	<owl:ObjectProperty rdf:about="&oe-pdt;diagnoses">
		<rdfs:label>diagnoses</rdfs:label>
		<rdfs:domain rdf:resource="&oe-pdt;DepressionRatingScale"/>
		<rdfs:range rdf:resource="&oe-pdt;MajorDepressiveDisorder"/>
		<owl:inverseOf rdf:resource="&oe-pdt;isDiagnosedBy"/>
		<skos:definition>the relationship between Depression Rating Scales and Major Depressive Disorder</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&oe-pdt;hasBrandName">
		<rdfs:subPropertyOf rdf:resource="&cmns-dsg;isSignifiedBy"/>
		<rdfs:label>has brand name</rdfs:label>
		<rdfs:range rdf:resource="&cmns-dsg;Name"/>
		<owl:inverseOf rdf:resource="&oe-pdt;isBrandNameOf"/>
		<skos:definition>is a brand name by which the object is known by</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&oe-pdt;hasDiagnosis">
		<rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
		<rdfs:label>has diagnosis</rdfs:label>
		<rdfs:domain rdf:resource="&oe-pdt;Patient"/>
		<rdfs:range rdf:resource="&oe-pdt;Diagnosis"/>
		<skos:definition>a subsection of &quot;experiences&quot; denoting specifically the psychiatric disorder that a patient suffers from</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&oe-pdt;hasGenome">
		<rdfs:label>has genome</rdfs:label>
		<rdfs:domain rdf:resource="&oe-pdt;Patient"/>
		<rdfs:range rdf:resource="&oe-pdt;Genome"/>
		<skos:definition>refers to the genetic data of a patient</skos:definition>
	</owl:ObjectProperty>
	
	<owl:DatatypeProperty rdf:about="&oe-pdt;hasName">
		<rdfs:subPropertyOf rdf:resource="&cmns-txt;hasTextValue"/>
	</owl:DatatypeProperty>
	
	<owl:ObjectProperty rdf:about="&oe-pdt;hasOnset">
		<rdfs:label>has onset</rdfs:label>
		<rdfs:domain rdf:resource="&oe-pdt;Treatment"/>
		<rdfs:range rdf:resource="&cmns-dt;Duration"/>
		<skos:definition>specifies the duration for which a treatment should be tested</skos:definition>
		<skos:editorialNote>The definition of &apos;onset&apos; from the NCI Thesaurus is &apos;the start, beginning, or early stages&apos; - see https://ncit.nci.nih.gov/ and query for &apos;onset&apos;. This doesn&apos;t seem to fit with having duration in the range.</skos:editorialNote>
		<cmns-av:explanatoryNote>If the duration ends without cessation of symptoms, then drug treatments should end and alternative treatments should be considered.</cmns-av:explanatoryNote>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&oe-pdt;hasResponse">
		<rdfs:label>has response</rdfs:label>
		<rdfs:domain rdf:resource="&oe-pdt;Patient"/>
		<rdfs:range rdf:resource="&oe-pdt;TreatmentResponse"/>
		<skos:definition>patients can have responses to treatments. Ideally positive responses.</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&oe-pdt;hasSymptom">
		<rdfs:subPropertyOf rdf:resource="&cmns-cls;isCharacterizedBy"/>
		<rdfs:label>has symptom</rdfs:label>
		<rdfs:range rdf:resource="&oe-pdt;Symptom"/>
		<owl:inverseOf rdf:resource="&oe-pdt;isSymptomOf"/>
		<skos:definition>psychiatric disorders have symptoms that manifest in the patient</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&oe-pdt;isAssociatedWith">
		<rdfs:label>is associated with</rdfs:label>
		<rdfs:domain rdf:resource="&oe-pdt;DiseaseAssociatedGene"/>
		<rdfs:range rdf:resource="&oe-pdt;PsychiatricDisorder"/>
		<skos:definition>placeholder the disorder with which these genes are associated.</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&oe-pdt;isBrandNameOf">
		<rdfs:subPropertyOf rdf:resource="&cmns-dsg;denotes"/>
		<rdfs:label>is brand name of</rdfs:label>
		<rdfs:domain rdf:resource="&cmns-dsg;Name"/>
		<skos:definition>brand names are attributed to specific drugs</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&oe-pdt;isDiagnosedBy">
		<rdfs:label>is diagnosed by</rdfs:label>
		<rdfs:domain rdf:resource="&oe-pdt;MajorDepressiveDisorder"/>
		<rdfs:range rdf:resource="&oe-pdt;DepressionRatingScale"/>
		<skos:definition>the relationship between a disorder and the methods by which a patient would be diagnosed with that disorder.</skos:definition>
		<skos:example>Depression Rating Scales are used to diagnose people with major depressive disorder.</skos:example>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&oe-pdt;isResponseTo">
		<rdfs:label>is response to</rdfs:label>
		<rdfs:domain rdf:resource="&oe-pdt;TreatmentResponse"/>
		<rdfs:range rdf:resource="&oe-pdt;Treatment"/>
		<skos:definition>treatment responses are responses that a patient has to treatments</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&oe-pdt;isSymptomOf">
		<rdfs:subPropertyOf rdf:resource="&cmns-cls;characterizes"/>
		<rdfs:label>is symptom of</rdfs:label>
		<rdfs:domain rdf:resource="&oe-pdt;Symptom"/>
		<skos:definition>refers to a disorder with which this symptom is associated</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&oe-pdt;preventsReuptakeOf">
		<rdfs:label>prevents reuptake of</rdfs:label>
		<rdfs:domain rdf:resource="&oe-pdt;ReuptakeInhibitor"/>
		<rdfs:range rdf:resource="&oe-pdt;Neurotransmitter"/>
		<skos:definition>reuptake inhibitors affect the ability for the brain to reuptake certain neurotransmitters.</skos:definition>
	</owl:ObjectProperty>
	
	<owl:ObjectProperty rdf:about="&oe-pdt;resistsTreatment">
		<rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
		<rdfs:label>resists treatment</rdfs:label>
		<rdfs:domain rdf:resource="&oe-pdt;PsychiatricDisorder"/>
		<rdfs:range rdf:resource="&oe-pdt;Treatment"/>
		<skos:definition>a disorder resists a treatment if the patient has been undergoing the treatment for its onset duration and has seen no cessation of symptoms.</skos:definition>
	</owl:ObjectProperty>
	
	<owl:NamedIndividual rdf:about="&oe-pdt;serotonin">
		<rdf:type rdf:resource="&oe-pdt;Neurotransmitter"/>
	</owl:NamedIndividual>
	
	<owl:Class rdf:about="&oe-pdt;substance">
		<rdfs:label>substance</rdfs:label>
		<owl:disjointWith rdf:resource="&cmns-pts;Agent"/>
		<skos:definition>material with particular physical characteristics</skos:definition>
	</owl:Class>
	
	<owl:ObjectProperty rdf:about="&oe-pdt;treats">
		<rdfs:label>treats</rdfs:label>
		<rdfs:domain rdf:resource="&oe-pdt;Treatment"/>
		<rdfs:range rdf:resource="&oe-pdt;PsychiatricDisorder"/>
		<skos:definition>placeholder for any treatment X and psychiatric disorder Y, X t Y if X mitigates the symptoms of Y</skos:definition>
	</owl:ObjectProperty>
	
	<rdf:Description rdf:about="&cmns-pts;Agent">
		<owl:disjointWith rdf:resource="&cmns-pts;Situation"/>
	</rdf:Description>

</rdf:RDF>