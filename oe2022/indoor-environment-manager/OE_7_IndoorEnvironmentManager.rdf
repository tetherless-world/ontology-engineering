# baseURI: https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/
# imports: https://www.omg.org/spec/Commons/AnnotationVocabulary/
# imports: https://www.omg.org/spec/Commons/Classifiers/
# imports: https://www.omg.org/spec/Commons/Collections/
# imports: https://www.omg.org/spec/Commons/DatesAndTimes/
# imports: https://www.omg.org/spec/Commons/Designators/
# imports: https://www.omg.org/spec/Commons/PartiesAndSituations/
# imports: https://www.omg.org/spec/Commons/QuantitiesAndUnits/

@base <https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/> .
@prefix : <https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/> .
@prefix cmns-av: <https://www.omg.org/spec/Commons/AnnotationVocabulary/> .
@prefix cmns-cls: <https://www.omg.org/spec/Commons/Classifiers/> .
@prefix cmns-col: <https://www.omg.org/spec/Commons/Collections/> .
@prefix cmns-dsg: <https://www.omg.org/spec/Commons/Designators/> .
@prefix cmns-dt: <https://www.omg.org/spec/Commons/DatesAndTimes/> .
@prefix cmns-pts: <https://www.omg.org/spec/Commons/PartiesAndSituations/> .
@prefix cmns-qtu: <https://www.omg.org/spec/Commons/QuantitiesAndUnits/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix oe2022-iem: <https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/>
	a owl:Ontology ;
	rdfs:label "Indoor Environment Manager Ontology" ;
	dct:abstract "" ;
	owl:imports
		<https://www.omg.org/spec/Commons/AnnotationVocabulary/> ,
		<https://www.omg.org/spec/Commons/Classifiers/> ,
		<https://www.omg.org/spec/Commons/Collections/> ,
		<https://www.omg.org/spec/Commons/DatesAndTimes/> ,
		<https://www.omg.org/spec/Commons/Designators/> ,
		<https://www.omg.org/spec/Commons/PartiesAndSituations/> ,
		<https://www.omg.org/spec/Commons/QuantitiesAndUnits/>
		;
	.

oe2022-iem:AirQuality
	a
		owl:NamedIndividual ,
		owl:Class ,
		cmns-qtu:QuantityKind
		;
	rdfs:subClassOf oe2022-iem:IndoorEnvironmentalQuality ;
	rdfs:label "air quality" ;
	owl:disjointWith oe2022-iem:ThermalComfort ;
	skos:definition "Synthesis of various physical measurements of properties of air that threaten human health" ;
	cmns-qtu:hasMeasurementUnit oe2022-iem:AirQualityIndex ;
	.

oe2022-iem:AirQualityAttribute
	a owl:Class ;
	rdfs:subClassOf oe2022-iem:EnvironmentAttribute ;
	.

oe2022-iem:AirQualityIndex
	a
		owl:NamedIndividual ,
		cmns-qtu:ArbitraryUnit
		;
	rdfs:label "air quality index" ;
	skos:definition "measurement that combines other meaurements of properties of air to indicate the level of danger to human health" ;
	.

oe2022-iem:AirSpeedAttribute
	a owl:Class ;
	rdfs:subClassOf
		oe2022-iem:DeltaCapableMeasurement ,
		oe2022-iem:EnvironmentAttribute
		;
	.

oe2022-iem:AirTemperatureAttribute
	a owl:Class ;
	rdfs:subClassOf
		oe2022-iem:DeltaCapableMeasurement ,
		oe2022-iem:EnvironmentAttribute
		;
	.

oe2022-iem:BaselineIndoorEnvironment
	a owl:Class ;
	rdfs:subClassOf
		oe2022-iem:IndoorEnvironment ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasAttribute ;
			owl:allValuesFrom [
				a owl:Class ;
				owl:complementOf oe2022-iem:EnvironmentAttributeDelta ;
			] ;
		]
		;
	owl:disjointWith oe2022-iem:ResultantIndoorEnvironment ;
	.

oe2022-iem:Bucket
	a owl:Class ;
	owl:equivalentClass [
		a owl:Class ;
		owl:oneOf (
			oe2022-iem:High
			oe2022-iem:Low
			oe2022-iem:Medium
		) ;
	] ;
	.

oe2022-iem:ClosedState
	a owl:Class ;
	rdfs:subClassOf oe2022-iem:ComponentState ;
	rdfs:label "closed state" ;
	owl:disjointWith oe2022-iem:OpenState ;
	skos:definition "condition of being not open" ;
	.

oe2022-iem:ComponentState
	a owl:Class ;
	rdfs:label "component state" ;
	skos:definition "an object in a room that can be used to alter relevant IEQ parameters" ;
	.

oe2022-iem:Cost
	a
		owl:NamedIndividual ,
		cmns-qtu:BaseQuantityKind
		;
	rdfs:label "cost" ;
	skos:definition "amount paid or charged for something" ;
	.

oe2022-iem:DaylightIntensityAttribute
	a owl:Class ;
	rdfs:subClassOf oe2022-iem:EnvironmentAttribute ;
	.

oe2022-iem:Dehumidifier
	a owl:Class ;
	rdfs:subClassOf
		oe2022-iem:Equipment ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasState ;
			owl:allValuesFrom [
				a owl:Class ;
				owl:unionOf (
					oe2022-iem:OffState
					oe2022-iem:OnState
				) ;
			] ;
		]
		;
	rdfs:label "dehumidifier" ;
	skos:definition "room component that can be on or off, thereby lowering indoor relative humidity or not, respectively" ;
	.

oe2022-iem:DeltaCapableMeasurement
	a owl:Class ;
	rdfs:subClassOf oe2022-iem:Measurement ;
	.

oe2022-iem:DeltaDefinedEnvironment
	a owl:Class ;
	rdfs:subClassOf
		oe2022-iem:Environment ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasAttribute ;
			owl:allValuesFrom [
				a owl:Class ;
				owl:unionOf (
					oe2022-iem:EnvironmentAttributeDelta
					[
						a owl:Class ;
						owl:complementOf [
							a owl:Class ;
							owl:intersectionOf (
								oe2022-iem:DeltaCapableMeasurement
								oe2022-iem:EnvironmentAttribute
							) ;
						] ;
					]
				) ;
			] ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:isDefinedInTermsOf ;
			owl:onClass oe2022-iem:BaselineIndoorEnvironment ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		]
		;
	.

oe2022-iem:Energy
	a
		owl:NamedIndividual ,
		cmns-qtu:BaseQuantityKind
		;
	rdfs:label "energy" ;
	skos:definition "measurement of the ability of something to do work" ;
	.

oe2022-iem:EnergyUsage
	a owl:Class ;
	rdfs:subClassOf
		oe2022-iem:Measurement ,
		[
			a owl:Restriction ;
			owl:onProperty cmns-qtu:hasMeasurementUnit ;
			owl:hasValue oe2022-iem:KilowattHour ;
		]
		;
	rdfs:label "energy usage" ;
	skos:definition "quantity of energy used to power some equipment, per unit time" ;
	.

oe2022-iem:Environment
	a owl:Class ;
	rdfs:subClassOf
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasAttribute ;
			owl:onClass oe2022-iem:AirQualityAttribute ;
			owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasAttribute ;
			owl:onClass oe2022-iem:AirSpeedAttribute ;
			owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasAttribute ;
			owl:onClass oe2022-iem:AirTemperatureAttribute ;
			owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasAttribute ;
			owl:onClass oe2022-iem:DaylightIntensityAttribute ;
			owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasAttribute ;
			owl:onClass oe2022-iem:RelativeHumidityAttribute ;
			owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		]
		;
	.

oe2022-iem:EnvironmentAttribute
	a owl:Class ;
	rdfs:subClassOf oe2022-iem:Measurement ;
	rdfs:label "environment attribute" ;
	skos:definition "a quantitative attribute of local environment" ;
	.

oe2022-iem:EnvironmentAttributeDelta
	a owl:Class ;
	rdfs:subClassOf [
		a owl:Restriction ;
		owl:onProperty oe2022-iem:hasSign ;
		owl:onClass oe2022-iem:Sign ;
		owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
	] ;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			oe2022-iem:DeltaCapableMeasurement
			oe2022-iem:EnvironmentAttribute
		) ;
	] ;
	.

oe2022-iem:Equipment
	a owl:Class ;
	rdfs:subClassOf
		oe2022-iem:RoomComponent ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasState ;
			owl:onClass oe2022-iem:ComponentState ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasAttribute ;
			owl:onClass oe2022-iem:EnergyUsage ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "equipment" ;
	owl:disjointWith oe2022-iem:Furniture ;
	skos:definition "room component that consumes energy" ;
	.

oe2022-iem:EquipmentEnergyUsage
	a owl:Class ;
	rdfs:label "equipment energy usage" ;
	skos:definition "Energy consumption used by equipment in a room" ;
	.

oe2022-iem:Fan
	a owl:Class ;
	rdfs:subClassOf
		oe2022-iem:Equipment ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasState ;
			owl:allValuesFrom [
				a owl:Class ;
				owl:unionOf (
					oe2022-iem:OffState
					oe2022-iem:OnState
				) ;
			] ;
		]
		;
	rdfs:label "fan" ;
	skos:definition "HVAC equipment that can increase air speed" ;
	.

oe2022-iem:Furniture
	a owl:Class ;
	rdfs:subClassOf
		oe2022-iem:RoomComponent ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasState ;
			owl:allValuesFrom [
				a owl:Class ;
				owl:unionOf (
					oe2022-iem:ClosedState
					oe2022-iem:OpenState
				) ;
			] ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasState ;
			owl:onClass oe2022-iem:ComponentState ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "furniture" ;
	skos:definition "room component that does not consume energy" ;
	.

oe2022-iem:Gram
	a
		owl:NamedIndividual ,
		cmns-qtu:BaseUnit
		;
	rdfs:label "gram" ;
	dct:source "https://ucum.org/ucum.html#section-Base-Units" ;
	skos:definition "base unit of mass defined in the Unified Code for Units Of Measure (UCUM) System of Units" ;
	cmns-av:explanatoryNote "The kilogram, as of 2019, is defined by the International Bureau of Weights and Measures from the fixed numerical value of the Planck constant (h), which is 6.62607015×10^−34 kg⋅m^2⋅s^−1" ;
	cmns-av:symbol "g" ;
	cmns-dsg:isDefinedIn cmns-qtu:InternationalSystemOfQuantities ;
	.

oe2022-iem:High
	a
		owl:NamedIndividual ,
		oe2022-iem:Bucket
		;
	.

oe2022-iem:Hour
	a
		owl:NamedIndividual ,
		cmns-qtu:ConversionBasedUnit ,
		cmns-qtu:UnitPowerFactor
		;
	rdfs:label "hour" ;
	skos:definition "unit of time representing the 24th part of a day" ;
	cmns-dsg:isDefinedIn cmns-qtu:InternationalSystemOfUnits ;
	cmns-qtu:hasExponent "1/1"^^owl:rational ;
	cmns-qtu:hasMeasurementUnit oe2022-iem:Hour ;
	cmns-qtu:hasQuantityKind oe2022-iem:Time ;
	.

oe2022-iem:IEQAirQualityEffect
	a owl:Class ;
	rdfs:subClassOf
		oe2022-iem:IEQEffect ,
		[
			a owl:Restriction ;
			owl:onProperty cmns-qtu:hasQuantityValue ;
			owl:someValuesFrom [
				a owl:Restriction ;
				owl:onProperty cmns-qtu:hasMeasurementUnit ;
				owl:someValuesFrom [
					a owl:Restriction ;
					owl:onProperty cmns-qtu:hasQuantityKind ;
					owl:hasValue oe2022-iem:AirQuality ;
				] ;
			] ;
		]
		;
	rdfs:label "indoor environment quality air quality effect" ;
	skos:definition "an air quality effect on the indoor environment quality" ;
	.

oe2022-iem:IEQAirSpeedEffect
	a owl:Class ;
	rdfs:subClassOf
		oe2022-iem:IEQEffect ,
		[
			a owl:Restriction ;
			owl:onProperty cmns-qtu:hasQuantityValue ;
			owl:someValuesFrom [
				a owl:Restriction ;
				owl:onProperty cmns-qtu:hasMeasurementUnit ;
				owl:someValuesFrom [
					a owl:Restriction ;
					owl:onProperty cmns-qtu:hasQuantityKind ;
					owl:hasValue oe2022-iem:Speed ;
				] ;
			] ;
		]
		;
	rdfs:label "indoor environment quality air speed effect" ;
	skos:definition "an air speed effect on the indoor environment quality" ;
	.

oe2022-iem:IEQAirTemperatureEffect
	a owl:Class ;
	rdfs:subClassOf
		oe2022-iem:IEQEffect ,
		[
			a owl:Restriction ;
			owl:onProperty cmns-qtu:hasQuantityValue ;
			owl:someValuesFrom [
				a owl:Restriction ;
				owl:onProperty cmns-qtu:hasMeasurementUnit ;
				owl:someValuesFrom [
					a owl:Restriction ;
					owl:onProperty cmns-qtu:hasQuantityKind ;
					owl:hasValue oe2022-iem:Temperature ;
				] ;
			] ;
		]
		;
	rdfs:label "indoor environment quality air temperature effect" ;
	skos:definition "an air temperature effect on the indoor environment quality" ;
	.

oe2022-iem:IEQEffect
	a owl:Class ;
	rdfs:subClassOf [
		a owl:Restriction ;
		owl:onProperty cmns-qtu:hasQuantityValue ;
		owl:someValuesFrom cmns-qtu:QuantityValue ;
	] ;
	rdfs:label "indoor environment quality effect" ;
	skos:definition "an effect on the indoor environment quality" ;
	.

oe2022-iem:IEQRelativeHumidityEffect
	a owl:Class ;
	rdfs:subClassOf
		oe2022-iem:IEQEffect ,
		[
			a owl:Restriction ;
			owl:onProperty cmns-qtu:hasQuantityValue ;
			owl:someValuesFrom [
				a owl:Restriction ;
				owl:onProperty cmns-qtu:hasMeasurementUnit ;
				owl:someValuesFrom [
					a owl:Restriction ;
					owl:onProperty cmns-qtu:hasQuantityKind ;
					owl:hasValue oe2022-iem:RelativeHumidity ;
				] ;
			] ;
		]
		;
	rdfs:label "indoor environment quality relative humidity effect" ;
	skos:definition "a relative humidity effect on the indoor environment quality" ;
	.

oe2022-iem:IndoorEnvironment
	a owl:Class ;
	rdfs:subClassOf
		oe2022-iem:Environment ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasAttribute ;
			owl:onClass oe2022-iem:DaylightIntensityAttribute ;
			owl:qualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		]
		;
	owl:disjointWith oe2022-iem:OutdoorEnvironment ;
	.

oe2022-iem:IndoorEnvironmentalQuality
	a owl:Class ;
	rdfs:label "indoor environmental quality" ;
	skos:definition "a perceived indoor experience of the building indoor including thermal comfort, indoor air quality, lighting, acoustics, and control systems" ;
	.

oe2022-iem:Kelvin
	a
		owl:NamedIndividual ,
		cmns-qtu:BaseUnit
		;
	rdfs:label "kelvin" ;
	dct:source "https://ucum.org/ucum.html#section-Base-Units" ;
	rdfs:seeAlso <https://www.nist.gov/pml/owm/si-units-temperature> ;
	skos:definition "base unit of temperature that is defined by taking the fixed numerical value of the Boltzmann constant k to be 1.380 649 ×10^−23 when expressed in the unit J K^−1, which is equal to kg m^2 s^−2 K^−1" ;
	cmns-av:explanatoryNote "The temperature 0 K is commonly referred to as 'absolute zero.' On the widely used Celsius temperature scale, water freezes at 0 °C and boils at about 100 °C. One Celsius degree is an interval of 1 K, and zero degrees Celsius is 273.15 K. An interval of one Celsius degree corresponds to an interval of 1.8 Fahrenheit degrees on the Fahrenheit temperature scale." ;
	cmns-av:symbol "K" ;
	cmns-dsg:isDefinedIn cmns-qtu:InternationalSystemOfUnits ;
	cmns-qtu:hasQuantityKind oe2022-iem:Temperature ;
	.

oe2022-iem:Kilo
	a
		owl:NamedIndividual ,
		cmns-qtu:Prefix
		;
	rdfs:label "kilo" ;
	dct:source "https://ucum.org/ucum.html#section-Prefixes" ;
	skos:definition "prefix defined in the Unified Code for Units Of Measure (UCUM) System of Units" ;
	cmns-av:symbol "k" ;
	cmns-dsg:hasDescription "10^3=(10^3)^1" ;
	cmns-dsg:isDefinedIn cmns-qtu:InternationalSystemOfQuantities ;
	.

oe2022-iem:Kilogram
	a
		owl:NamedIndividual ,
		cmns-qtu:PrefixedUnit
		;
	rdfs:label "kilogram" ;
	dct:source "https://ucum.org/ucum.html#section-Base-Units" ;
	rdfs:seeAlso <https://www.nist.gov/si-redefinition/kilogram> ;
	skos:definition "conversion-based unit of mass" ;
	skos:note "Note that in the International System of Units (SI), the kilogram is considered the base unit rather than the gram, which is the base unit in the UCUM." ;
	cmns-av:symbol "kg" ;
	cmns-dsg:hasDescription "10^3 grams" ;
	cmns-dsg:isDefinedIn cmns-qtu:InternationalSystemOfUnits ;
	cmns-qtu:hasFactor oe2022-iem:Kilo ;
	cmns-qtu:hasQuantityKind oe2022-iem:Mass ;
	cmns-qtu:isDerivedFrom oe2022-iem:Gram ;
	.

oe2022-iem:Kilowatt
	a
		owl:NamedIndividual ,
		cmns-qtu:PrefixedUnit
		;
	rdfs:label "kilowatt" ;
	skos:definition "conversion-based unit of energy" ;
	cmns-dsg:isDefinedIn cmns-qtu:InternationalSystemOfUnits ;
	cmns-qtu:hasFactor oe2022-iem:Kilo ;
	cmns-qtu:hasQuantityKind oe2022-iem:Energy ;
	cmns-qtu:isDerivedFrom oe2022-iem:Watt ;
	.

oe2022-iem:KilowattHour
	a
		owl:NamedIndividual ,
		cmns-qtu:DerivedUnit
		;
	rdfs:label "kilowatt-hour" ;
	skos:definition "measure of electrical energy equivalent to a power consumption of 1,000 watts for 1 hour" ;
	cmns-dsg:isDefinedIn cmns-qtu:InternationalSystemOfUnits ;
	cmns-qtu:hasFactor oe2022-iem:Hour ;
	cmns-qtu:hasQuantityKind oe2022-iem:Energy ;
	cmns-qtu:isDerivedFrom oe2022-iem:Kilowatt ;
	.

oe2022-iem:Length
	a
		owl:NamedIndividual ,
		cmns-qtu:BaseQuantityKind
		;
	rdfs:label "length" ;
	dct:source "https://ucum.org/ucum.html#section-Base-Units" ;
	skos:definition "base quantity kind that is the quantitative measure of distance, i.e., of how far apart objects or points are" ;
	cmns-av:symbol "l" ;
	cmns-dsg:isDefinedIn cmns-qtu:InternationalSystemOfQuantities ;
	.

oe2022-iem:Low
	a
		owl:NamedIndividual ,
		oe2022-iem:Bucket
		;
	.

oe2022-iem:Mass
	a
		owl:NamedIndividual ,
		cmns-qtu:BaseQuantityKind
		;
	rdfs:label "mass" ;
	skos:definition "base quantity kind for the measure of a measure of a body's inertial property or how much matter it contains" ;
	cmns-av:explanatoryNote "Note that as of 2018, mass has been redefined in the International System of Units in terms of Planck's constant, h. The kilogram is defined by taking the fixed numerical value of the Planck constant, h, to be 6.626 070 15 × 10-34 when expressed in the unit J s, which is equal to kg m^2 s^−1, where the metre and the second are defined in terms of the speed of light, 𝒸, and the hyperfine transition frequency of the caesium-133 atom, ∆ν, respectively." ;
	cmns-av:symbol "m" ;
	cmns-dsg:isDefinedIn cmns-qtu:InternationalSystemOfQuantities ;
	.

oe2022-iem:Measurement
	a owl:Class ;
	rdfs:subClassOf
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasBucket ;
			owl:onClass oe2022-iem:Bucket ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty cmns-qtu:hasMeasurementUnit ;
			owl:someValuesFrom cmns-qtu:MeasurementUnit ;
		]
		;
	rdfs:label "measurement" ;
	skos:definition "amount of something" ;
	.

oe2022-iem:Medium
	a
		owl:NamedIndividual ,
		oe2022-iem:Bucket
		;
	.

oe2022-iem:Meter
	a
		owl:NamedIndividual ,
		cmns-qtu:BaseUnit
		;
	rdfs:label "meter" ;
	dct:source "https://ucum.org/ucum.html#section-Base-Units" ;
	rdfs:seeAlso
		<https://www.nist.gov/pml/owm/si-units-length> ,
		<https://www.nist.gov/system/files/documents/pml/div683/museum-length.pdf>
		;
	skos:definition "base unit of length that is the distance traveled by light in vacuum during the time interval of 1/299 792 458 of a second" ;
	cmns-av:symbol "m" ;
	cmns-dsg:isDefinedIn cmns-qtu:InternationalSystemOfUnits ;
	cmns-qtu:hasQuantityKind oe2022-iem:Length ;
	.

oe2022-iem:MetersPerSecond
	a
		owl:NamedIndividual ,
		cmns-qtu:DerivedUnit
		;
	rdfs:label "meter per second" ;
	skos:definition "measure of speed equipvalent to the moved meters for 1 second" ;
	cmns-dsg:isDefinedIn cmns-qtu:InternationalSystemOfUnits ;
	cmns-qtu:hasFactor oe2022-iem:PerSecond ;
	cmns-qtu:hasQuantityKind oe2022-iem:Speed ;
	cmns-qtu:isDerivedFrom oe2022-iem:Meter ;
	.

oe2022-iem:Negative
	a
		owl:NamedIndividual ,
		oe2022-iem:Sign
		;
	.

oe2022-iem:Occupant
	a owl:Class ;
	rdfs:subClassOf
		cmns-pts:PartyRole ,
		[
			a owl:Restriction ;
			owl:onProperty cmns-pts:isPlayedBy ;
			owl:onClass oe2022-iem:Person ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		]
		;
