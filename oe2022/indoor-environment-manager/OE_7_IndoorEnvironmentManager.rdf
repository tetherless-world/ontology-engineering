@prefix : <https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix cmns-av: <https://www.omg.org/spec/Commons/AnnotationVocabulary/> .
@prefix cmns-dt: <https://www.omg.org/spec/Commons/DatesAndTimes/> .
@prefix cmns-cls: <https://www.omg.org/spec/Commons/Classifiers/> .
@prefix cmns-col: <https://www.omg.org/spec/Commons/Collections/> .
@prefix cmns-dsg: <https://www.omg.org/spec/Commons/Designators/> .
@prefix cmns-pts: <https://www.omg.org/spec/Commons/PartiesAndSituations/> .
@prefix cmns-qtu: <https://www.omg.org/spec/Commons/QuantitiesAndUnits/> .
@prefix oe2022-iem: <https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/> .
@base <https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/> .

<https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/> rdf:type owl:Ontology ;
                                                                              owl:imports cmns-av: ,
                                                                                          cmns-cls: ,
                                                                                          cmns-col: ,
                                                                                          cmns-dt: ,
                                                                                          cmns-dsg: ,
                                                                                          cmns-pts: ;
                                                                              dct:abstract "" ;
                                                                              rdfs:label "Indoor Environment Manager Ontology" .

#################################################################
#    Object Properties
#################################################################

###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/hasAttribute
oe2022-iem:hasAttribute rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf owl:topObjectProperty ;
                        owl:inverseOf oe2022-iem:isAttributeOf ;
                        rdfs:label "has attribute" ;
                        skos:definition "has components that affect the nature of the whole with respect to relevant actors" .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/hasBucket
oe2022-iem:hasBucket rdf:type owl:ObjectProperty ;
                     rdfs:range oe2022-iem:Bucket .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/hasEffect
oe2022-iem:hasEffect rdf:type owl:ObjectProperty ;
                     rdfs:label "has effect" ;
                     skos:definition "specify how to achange the parameters of indoor environmental quality" .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/hasMeasurementUnit
oe2022-iem:hasMeasurementUnit rdf:type owl:ObjectProperty ;
                              rdfs:range oe2022-iem:MeasurementUnit ;
                              rdfs:label "has measurement unit" ;
                              skos:definition "indicates the unit in which something is expressed" .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/hasPossibleState
oe2022-iem:hasPossibleState rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf owl:topObjectProperty ;
                            owl:inverseOf oe2022-iem:isPossibleStateOf ;
                            rdfs:domain oe2022-iem:RoomComponent ;
                            rdfs:range oe2022-iem:ComponentState ;
                            rdfs:label "has possible state" ;
                            skos:definition "has the ability to be in some condition at a point in time" .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/hasQuantityKind
oe2022-iem:hasQuantityKind rdf:type owl:ObjectProperty ;
                           rdfs:range oe2022-iem:QuantityKind ;
                           rdfs:label "has quantity kind" ;
                           skos:definition "indicates the mutually comparable quantities involves in the definitions of an individual quantity or factor" .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/hasSign
oe2022-iem:hasSign rdf:type owl:ObjectProperty ;
                   rdfs:range oe2022-iem:Sign .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/hasState
oe2022-iem:hasState rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    owl:inverseOf oe2022-iem:isStateOf ;
                    rdfs:domain oe2022-iem:RoomComponent ;
                    rdfs:range oe2022-iem:ComponentState ;
                    rdfs:label "has state" ;
                    skos:definition "is currently in the condition, referring to objects that have some changeable condition" .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/isAttributeOf
oe2022-iem:isAttributeOf rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf owl:topObjectProperty ;
                         rdfs:label "is attribute of" ;
                         skos:definition "is a component that affects the nature of the whole with respect to relevant actors" .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/isPossibleStateOf
oe2022-iem:isPossibleStateOf rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf owl:topObjectProperty ;
                             rdfs:label "is possible state of" ;
                             skos:definition "can be the condition of a given object at some point in time" .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/isQuantifiedBy
oe2022-iem:isQuantifiedBy rdf:type owl:ObjectProperty ;
                          owl:inverseOf oe2022-iem:quatifies ;
                          rdfs:label "is quantified by" ;
                          skos:definition "is specified by the quantify of the object" .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/isStateOf
oe2022-iem:isStateOf rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf owl:topObjectProperty ;
                     rdfs:label "is currently the condition of" ,
                                "is state of" .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/quatifies
oe2022-iem:quatifies rdf:type owl:ObjectProperty ;
                     rdfs:label "quantifies" ;
                     skos:definition "specifies the quantify of the object" .


###  https://www.omg.org/spec/Commons/QuantitiesAndUnits/hasLowerBound
cmns-qtu:hasLowerBound rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf oe2022-iem:hasAttribute ;
                       rdfs:range oe2022-iem:Argument ;
                       rdfs:label "has lower bound" ;
                       skos:definition "specifies the quantity value that is the lower value of a pair of values representing a range" .


###  https://www.omg.org/spec/Commons/QuantitiesAndUnits/hasUpperBound
cmns-qtu:hasUpperBound rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf oe2022-iem:hasAttribute ;
                       rdfs:range oe2022-iem:Argument ;
                       rdfs:label "has upper bound" ;
                       skos:definition "specifies the quantity value that is the higher value of a pair of values representing a range" .


#################################################################
#    Data properties
#################################################################

###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/hasNumericValue
oe2022-iem:hasNumericValue rdf:type owl:DatatypeProperty ;
                           rdfs:range xsd:int ;
                           rdfs:label "has numeric value" ;
                           skos:definition "indicates a particular magnitude or designation for a given observable characteristic that is a number" .


#################################################################
#    Classes
#################################################################

###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/AirQuality
oe2022-iem:AirQuality rdf:type owl:Class ;
                      rdfs:subClassOf oe2022-iem:IndoorEnvironmentalQuality ;
                      owl:disjointWith oe2022-iem:ThermalComfort .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/AirQualityAttribute
oe2022-iem:AirQualityAttribute rdf:type owl:Class ;
                               rdfs:subClassOf oe2022-iem:EnvironmentAttribute .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/AirSpeedAttribute
oe2022-iem:AirSpeedAttribute rdf:type owl:Class ;
                             rdfs:subClassOf oe2022-iem:DeltaCapableMeasurement ,
                                             oe2022-iem:EnvironmentAttribute .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/AirTemperatureAttribute
oe2022-iem:AirTemperatureAttribute rdf:type owl:Class ;
                                   rdfs:subClassOf oe2022-iem:DeltaCapableMeasurement ,
                                                   oe2022-iem:EnvironmentAttribute .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/Argument
oe2022-iem:Argument rdf:type owl:Class ;
                    rdfs:subClassOf [ rdf:type owl:Restriction ;
                                      owl:onProperty oe2022-iem:hasNumericValue ;
                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onDataRange xsd:int
                                    ] ;
                    rdfs:label "argument" .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/BaselineIndoorEnvironment
oe2022-iem:BaselineIndoorEnvironment rdf:type owl:Class ;
                                     rdfs:subClassOf oe2022-iem:IndoorEnvironment ,
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty oe2022-iem:hasAttribute ;
                                                       owl:allValuesFrom [ rdf:type owl:Class ;
                                                                           owl:complementOf oe2022-iem:EnvironmentAttributeDelta
                                                                         ]
                                                     ] ;
                                     owl:disjointWith oe2022-iem:ResultantIndoorEnvironment .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/Bucket
oe2022-iem:Bucket rdf:type owl:Class ;
                  owl:equivalentClass [ rdf:type owl:Class ;
                                        owl:oneOf ( oe2022-iem:High
                                                    oe2022-iem:Low
                                                    oe2022-iem:Medium
                                                  )
                                      ] .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/ComponentState
oe2022-iem:ComponentState rdf:type owl:Class ;
                          rdfs:label "component state" ;
                          skos:definition "an object in a room that can be used to alter relevant IEQ parameters" .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/DaylightIntensityAttribute
oe2022-iem:DaylightIntensityAttribute rdf:type owl:Class ;
                                      rdfs:subClassOf oe2022-iem:EnvironmentAttribute .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/Dehumidifier
oe2022-iem:Dehumidifier rdf:type owl:Class ;
                        rdfs:subClassOf oe2022-iem:Equipment ;
                        rdfs:label "dehumidifier" ;
                        skos:definition "room component that can be on or off, thereby lowering indoor relative humidity or not, respectively" .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/DeltaCapableMeasurement
oe2022-iem:DeltaCapableMeasurement rdf:type owl:Class ;
                                   rdfs:subClassOf oe2022-iem:Measurement .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/DeltaDefinedEnvironment
oe2022-iem:DeltaDefinedEnvironment rdf:type owl:Class ;
                                   rdfs:subClassOf oe2022-iem:Environment ,
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty oe2022-iem:hasAttribute ;
                                                     owl:allValuesFrom [ rdf:type owl:Class ;
                                                                         owl:unionOf ( oe2022-iem:EnvironmentAttributeDelta
                                                                                       [ rdf:type owl:Class ;
                                                                                         owl:complementOf [ owl:intersectionOf ( oe2022-iem:DeltaCapableMeasurement
                                                                                                                                 oe2022-iem:EnvironmentAttribute
                                                                                                                               ) ;
                                                                                                            rdf:type owl:Class
                                                                                                          ]
                                                                                       ]
                                                                                     )
                                                                       ]
                                                   ] .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/EnergyUsage
oe2022-iem:EnergyUsage rdf:type owl:Class ;
                       rdfs:subClassOf oe2022-iem:Measurement ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty oe2022-iem:hasMeasurementUnit ;
                                         owl:hasValue oe2022-iem:KilowattHour
                                       ] ;
                       rdfs:label "energy usage" ;
                       skos:definition "quantity of energy used to power some equipment, per unit time" .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/Environment
oe2022-iem:Environment rdf:type owl:Class ;
                       rdfs:subClassOf [ rdf:type owl:Restriction ;
                                         owl:onProperty oe2022-iem:hasAttribute ;
                                         owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onClass oe2022-iem:AirQualityAttribute
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty oe2022-iem:hasAttribute ;
                                         owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onClass oe2022-iem:AirSpeedAttribute
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty oe2022-iem:hasAttribute ;
                                         owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onClass oe2022-iem:AirTemperatureAttribute
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty oe2022-iem:hasAttribute ;
                                         owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onClass oe2022-iem:DaylightIntensityAttribute
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty oe2022-iem:hasAttribute ;
                                         owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onClass oe2022-iem:RelativeHumidityAttribute
                                       ] .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/EnvironmentAttribute
oe2022-iem:EnvironmentAttribute rdf:type owl:Class ;
                                rdfs:subClassOf oe2022-iem:Measurement ;
                                rdfs:label "environment attribute" ;
                                skos:definition "a quantitative attribute of local environment" .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/EnvironmentAttributeDelta
oe2022-iem:EnvironmentAttributeDelta rdf:type owl:Class ;
                                     owl:equivalentClass [ owl:intersectionOf ( oe2022-iem:DeltaCapableMeasurement
                                                                                oe2022-iem:EnvironmentAttribute
                                                                              ) ;
                                                           rdf:type owl:Class
                                                         ] ;
                                     rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                       owl:onProperty oe2022-iem:hasSign ;
                                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                       owl:onClass oe2022-iem:Sign
                                                     ] .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/Equipment
oe2022-iem:Equipment rdf:type owl:Class ;
                     rdfs:subClassOf oe2022-iem:RoomComponent ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty oe2022-iem:hasAttribute ;
                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onClass oe2022-iem:EnergyUsage
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty oe2022-iem:hasState ;
                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onClass oe2022-iem:ComponentState
                                     ] ;
                     owl:disjointWith oe2022-iem:Furniture ;
                     rdfs:label "equipment" ;
                     skos:definition "room component that consumes energy" .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/Fan
oe2022-iem:Fan rdf:type owl:Class ;
               rdfs:subClassOf oe2022-iem:Equipment ;
               rdfs:label "fan" ;
               skos:definition "HVAC equipment that can increase air speed" .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/Furniture
oe2022-iem:Furniture rdf:type owl:Class ;
                     rdfs:subClassOf oe2022-iem:RoomComponent ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty oe2022-iem:hasState ;
                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onClass oe2022-iem:ComponentState
                                     ] ;
                     rdfs:label "furniture" ;
                     skos:definition "room component that does not consume energy" .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/IndoorEnvironment
oe2022-iem:IndoorEnvironment rdf:type owl:Class ;
                             rdfs:subClassOf oe2022-iem:Environment ,
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty oe2022-iem:hasAttribute ;
                                               owl:qualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                                               owl:onClass oe2022-iem:DaylightIntensityAttribute
                                             ] ;
                             owl:disjointWith oe2022-iem:OutdoorEnvironment .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/IndoorEnvironmentalQuality
oe2022-iem:IndoorEnvironmentalQuality rdf:type owl:Class ;
                                      rdfs:label "indoor environmental quality" ;
                                      skos:definition "a perceived indoor experience of the building indoor including thermal comfort, indoor air quality, lighting, acoustics, and control systems" .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/Measurement
oe2022-iem:Measurement rdf:type owl:Class ;
                       rdfs:subClassOf [ rdf:type owl:Restriction ;
                                         owl:onProperty oe2022-iem:hasMeasurementUnit ;
                                         owl:someValuesFrom oe2022-iem:MeasurementUnit
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty oe2022-iem:hasBucket ;
                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onClass oe2022-iem:Bucket
                                       ] ;
                       rdfs:label "measurement" ;
                       skos:definition "amount of something" .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/MeasurementUnit
oe2022-iem:MeasurementUnit rdf:type owl:Class ;
                           rdfs:label "measurement unit" ;
                           skos:definition "real scalar quantity, defined and adopted by convention, with which any other quantity of the same kind can be compared to express the ratio of the second quantity to the first one as a number" .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/Occupant
oe2022-iem:Occupant rdf:type owl:Class ;
                    rdfs:subClassOf cmns-pts:PartyRole ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty cmns-pts:isPlayedBy ;
                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass oe2022-iem:Person
                                    ] ;
                    rdfs:label "occupant"@en ;
                    skos:definition "someone that resides or is present in or using a building, vehicle, seat, place, etc., at a given time"@en .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/Organization
oe2022-iem:Organization rdf:type owl:Class ;
                        rdfs:subClassOf cmns-pts:Party ;
                        rdfs:label "organization"@en ;
                        rdfs:seeAlso <https://www.w3.org/TR/vocab-org/#org:Organization> ;
                        skos:definition "collection of one or more people, or groups of people formed together into a community or other social, commercial or political structure to act, or that is designated to act, towards some purpose, such as to meet a need or pursue collective goals on a continuing basis"@en ;
                        skos:example "This may be a business entity, government, international organization, not-for-profit, academic institution, or other unincorporated and/or informal social organization."@en ;
                        cmns-av:adaptedFrom "https://www.iso.org/obp/ui/#iso:std:iso-iec:6523:-1:ed-1:v1:en" .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/OutdoorEnvironment
oe2022-iem:OutdoorEnvironment rdf:type owl:Class ;
                              rdfs:subClassOf oe2022-iem:DeltaDefinedEnvironment ,
                                              oe2022-iem:Environment .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/Person
oe2022-iem:Person rdf:type owl:Class ;
                  rdfs:subClassOf cmns-pts:Party ;
                  rdfs:label "person"@en ;
                  skos:definition "individual human being, with consciousness of self"@en ;
                  cmns-av:synonym "natural person"@en .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/QuantityKind
oe2022-iem:QuantityKind rdf:type owl:Class ;
                        rdfs:label "quantity kind" ;
                        skos:definition "aspect common to mutually comparable quantities" .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/Radiator
oe2022-iem:Radiator rdf:type owl:Class ;
                    rdfs:subClassOf oe2022-iem:Equipment ;
                    rdfs:label "radiator" ;
                    skos:definition "HVAC equipment that can raise the indoor temperature by transferring heat from an internal system into a room" .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/RelativeHumidityAttribute
oe2022-iem:RelativeHumidityAttribute rdf:type owl:Class ;
                                     rdfs:subClassOf oe2022-iem:DeltaCapableMeasurement ,
                                                     oe2022-iem:EnvironmentAttribute .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/ResultantIndoorEnvironment
oe2022-iem:ResultantIndoorEnvironment rdf:type owl:Class ;
                                      rdfs:subClassOf oe2022-iem:DeltaDefinedEnvironment ,
                                                      oe2022-iem:IndoorEnvironment .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/Room
oe2022-iem:Room rdf:type owl:Class ;
                rdfs:label "room" ;
                skos:definition "region in a building in which occupant comfort and measurements apply" .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/RoomComponent
oe2022-iem:RoomComponent rdf:type owl:Class ;
                         rdfs:label "room component" ;
                         skos:definition "an object in a room that can be used to alter relevant IEQ parameters" .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/Sign
oe2022-iem:Sign rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Class ;
                                      owl:oneOf ( oe2022-iem:Negative
                                                  oe2022-iem:Positive
                                                )
                                    ] .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/ThermalComfort
oe2022-iem:ThermalComfort rdf:type owl:Class ;
                          rdfs:subClassOf oe2022-iem:IndoorEnvironmentalQuality ;
                          rdfs:label "thermal comfort" ;
                          skos:definition "the condition of mind which expresses satisfaction with the surrounding thermal environment and is assessed by subjective evaluation" .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/Thermostat
oe2022-iem:Thermostat rdf:type owl:Class ;
                      rdfs:subClassOf oe2022-iem:Furniture ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty oe2022-iem:hasState ;
                                        owl:someValuesFrom oe2022-iem:ThermostatState
                                      ] .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/ThermostatState
oe2022-iem:ThermostatState rdf:type owl:Class ;
                           rdfs:subClassOf oe2022-iem:ComponentState ,
                                           [ rdf:type owl:Restriction ;
                                             owl:onProperty oe2022-iem:hasNumericValue ;
                                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                             owl:onDataRange xsd:int
                                           ] ;
                           rdfs:label "thermostat state" ;
                           skos:definition "numeric value at which the thermostat is set" .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/Window
oe2022-iem:Window rdf:type owl:Class ;
                  rdfs:subClassOf oe2022-iem:Furniture ;
                  owl:disjointWith oe2022-iem:WindowBlinds ;
                  rdfs:label "window" ;
                  skos:definition "room component that can optionally let air flow between inside and outside regions" .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/WindowBlinds
oe2022-iem:WindowBlinds rdf:type owl:Class ;
                        rdfs:subClassOf oe2022-iem:Furniture ;
                        rdfs:label "window blinds" ;
                        skos:definition "room component that be open or closed, thereby letting in or blocking light, respectively" .


###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/heating,_ventilation,_and_air_conditioning
<https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/heating,_ventilation,_and_air_conditioning> rdf:type owl:Class ;
                                                                                                                       rdfs:subClassOf oe2022-iem:Equipment ;
                                                                                                                       rdfs:label "heating, ventilation, and air conditioning" ;
                                                                                                                       skos:definition "equipment, distribution systems, and terminals that provide, either collectively or individually, the processes of heating, ventilating, or air conditioning to a building or portion of a building" .


#################################################################
#    Individuals
#################################################################

###  https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/AirQuality
oe2022-iem:AirQuality rdf:type owl:NamedIndividual ,
