oe2022-iem:Temperature
	a
		owl:NamedIndividual ,
		cmns-qtu:BaseQuantityKind
		;
	rdfs:label "temperature" ;
	dct:source "https://ucum.org/ucum.html#section-Base-Units" ;
	skos:definition "base quantity kind that is the quantitative measure of the ability of a substance, or more generally of any physical system, to transfer heat energy to another physical system" ;
	cmns-av:explanatoryNote "There are three types of temperature scale: those, such as the SI scale, that are defined in terms of the average translational kinetic energy per freely moving microscopic particle, such as atom, molecule, or electron, in a body; those that rely solely on purely macroscopic properties and thermodynamic principles, such as Kelvin's original definition; and those that are not defined by theoretical principles, but are defined by convenient empirical properties of particular materials." ;
	cmns-av:symbol "T" ;
	cmns-dsg:isDefinedIn cmns-qtu:InternationalSystemOfQuantities ;
	.

oe2022-iem:ThermalComfort
	a owl:Class ;
	rdfs:subClassOf oe2022-iem:IndoorEnvironmentalQuality ;
	rdfs:label "thermal comfort" ;
	skos:definition "the condition of mind which expresses satisfaction with the surrounding thermal environment and is assessed by subjective evaluation" ;
	.

oe2022-iem:Thermostat
	a owl:Class ;
	rdfs:subClassOf
		<heating,_ventilation,_and_air_conditioning> ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasState ;
			owl:someValuesFrom oe2022-iem:ThermostatState ;
		]
		;
