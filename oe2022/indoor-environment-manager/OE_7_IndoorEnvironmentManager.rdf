oe2022-iem:PerHour
	a
		owl:NamedIndividual ,
		cmns-qtu:UnitPowerFactor
		;
	cmns-qtu:hasExponent "-1/1"^^owl:rational ;
	cmns-qtu:hasMeasurementUnit oe2022-iem:Hour ;
