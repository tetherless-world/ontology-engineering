oe2022-iem:SquareLength
	a
		owl:NamedIndividual ,
		cmns-qtu:QuantityKindPowerFactor
		;
	cmns-qtu:hasExponent "2/1"^^owl:rational ;
	cmns-qtu:hasQuantityKind oe2022-iem:Length ;
	.

oe2022-iem:SquareMeter
	a
		owl:NamedIndividual ,
		cmns-qtu:UnitPowerFactor
		;
