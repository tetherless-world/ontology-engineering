			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasState ;
			owl:allValuesFrom [
				a owl:Class ;
				owl:unionOf (
					oe2022-iem:OffState
					oe2022-iem:OnState
				) ;
			] ;
		]
		;
