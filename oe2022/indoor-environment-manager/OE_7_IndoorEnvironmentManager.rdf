# baseURI: https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/
# imports: https://www.omg.org/spec/Commons/AnnotationVocabulary/
# imports: https://www.omg.org/spec/Commons/Classifiers/
# imports: https://www.omg.org/spec/Commons/Collections/
# imports: https://www.omg.org/spec/Commons/DatesAndTimes/
# imports: https://www.omg.org/spec/Commons/Designators/
# imports: https://www.omg.org/spec/Commons/PartiesAndSituations/

@base <https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/> .
@prefix : <https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/> .
@prefix cmns-av: <https://www.omg.org/spec/Commons/AnnotationVocabulary/> .
@prefix cmns-cls: <https://www.omg.org/spec/Commons/Classifiers/> .
@prefix cmns-col: <https://www.omg.org/spec/Commons/Collections/> .
@prefix cmns-dsg: <https://www.omg.org/spec/Commons/Designators/> .
@prefix cmns-dt: <https://www.omg.org/spec/Commons/DatesAndTimes/> .
@prefix cmns-pts: <https://www.omg.org/spec/Commons/PartiesAndSituations/> .
@prefix cmns-qtu: <https://www.omg.org/spec/Commons/QuantitiesAndUnits/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix oe2022-iem: <https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://tw.rpi.edu/ontology-engineering/oe2022/indoor-environment-manager/>
	a owl:Ontology ;
	rdfs:label "Indoor Environment Manager Ontology" ;
	dct:abstract "" ;
	owl:imports
		<https://www.omg.org/spec/Commons/AnnotationVocabulary/> ,
		<https://www.omg.org/spec/Commons/Classifiers/> ,
		<https://www.omg.org/spec/Commons/Collections/> ,
		<https://www.omg.org/spec/Commons/DatesAndTimes/> ,
		<https://www.omg.org/spec/Commons/Designators/> ,
		<https://www.omg.org/spec/Commons/PartiesAndSituations/>
		;
	.

oe2022-iem:AirQuality
	a
		owl:NamedIndividual ,
		owl:Class ,
		oe2022-iem:QuantityKind
		;
	rdfs:subClassOf oe2022-iem:IndoorEnvironmentalQuality ;
	rdfs:label "air quality" ;
	owl:disjointWith oe2022-iem:ThermalComfort ;
	skos:definition "Synthesis of various physical measurements of properties of air that threaten human health" ;
	.

oe2022-iem:AirQualityAttribute
	a owl:Class ;
	rdfs:subClassOf oe2022-iem:EnvironmentAttribute ;
	.

oe2022-iem:AirQualityIndex
	a
		owl:NamedIndividual ,
		oe2022-iem:MeasurementUnit
		;
	rdfs:label "air quality index" ;
	skos:definition "measurement that combines other meaurements of properties of air to indicate the level of danger to human health" ;
	.

oe2022-iem:AirSpeedAttribute
	a owl:Class ;
	rdfs:subClassOf
		oe2022-iem:DeltaCapableMeasurement ,
		oe2022-iem:EnvironmentAttribute
		;
	.

oe2022-iem:AirTemperatureAttribute
	a owl:Class ;
	rdfs:subClassOf
		oe2022-iem:DeltaCapableMeasurement ,
		oe2022-iem:EnvironmentAttribute
		;
	.

oe2022-iem:Argument
	a owl:Class ;
	rdfs:subClassOf [
		a owl:Restriction ;
		owl:onProperty oe2022-iem:hasNumericValue ;
		owl:onDataRange xsd:int ;
		owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
	] ;
	rdfs:label "argument" ;
	.

oe2022-iem:BaselineIndoorEnvironment
	a owl:Class ;
	rdfs:subClassOf
		oe2022-iem:IndoorEnvironment ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasAttribute ;
			owl:allValuesFrom [
				a owl:Class ;
				owl:complementOf oe2022-iem:EnvironmentAttributeDelta ;
			] ;
		]
		;
	owl:disjointWith oe2022-iem:ResultantIndoorEnvironment ;
	.

oe2022-iem:Bucket
	a owl:Class ;
	owl:equivalentClass [
		a owl:Class ;
		owl:oneOf (
			oe2022-iem:High
			oe2022-iem:Low
			oe2022-iem:Medium
		) ;
	] ;
	.

oe2022-iem:ClosedState
	a owl:Class ;
	rdfs:subClassOf oe2022-iem:ComponentState ;
	rdfs:label "closed state" ;
	owl:disjointWith oe2022-iem:OpenState ;
	skos:definition "condition of being not open" ;
	.

oe2022-iem:ComponentState
	a owl:Class ;
	rdfs:label "component state" ;
	skos:definition "an object in a room that can be used to alter relevant IEQ parameters" ;
	.

oe2022-iem:DaylightIntensityAttribute
	a owl:Class ;
	rdfs:subClassOf oe2022-iem:EnvironmentAttribute ;
	.

oe2022-iem:Dehumidifier
	a owl:Class ;
	rdfs:subClassOf
		oe2022-iem:Equipment ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasState ;
			owl:allValuesFrom [
				a owl:Class ;
				owl:unionOf (
					oe2022-iem:OffState
					oe2022-iem:OnState
				) ;
			] ;
		]
		;
	rdfs:label "dehumidifier" ;
	skos:definition "room component that can be on or off, thereby lowering indoor relative humidity or not, respectively" ;
	.

oe2022-iem:DeltaCapableMeasurement
	a owl:Class ;
	rdfs:subClassOf oe2022-iem:Measurement ;
	.

oe2022-iem:DeltaDefinedEnvironment
	a owl:Class ;
	rdfs:subClassOf
		oe2022-iem:Environment ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasAttribute ;
			owl:allValuesFrom [
				a owl:Class ;
				owl:unionOf (
					oe2022-iem:EnvironmentAttributeDelta
					[
						a owl:Class ;
						owl:complementOf [
							a owl:Class ;
							owl:intersectionOf (
								oe2022-iem:DeltaCapableMeasurement
								oe2022-iem:EnvironmentAttribute
							) ;
						] ;
					]
				) ;
			] ;
		]
		;
	.

oe2022-iem:Energy
	a
		owl:NamedIndividual ,
		oe2022-iem:QuantityKind
		;
	rdfs:label "energy" ;
	skos:definition "measurement of the ability of something to do work" ;
	.

oe2022-iem:EnergyUsage
	a owl:Class ;
	rdfs:subClassOf
		oe2022-iem:Measurement ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasMeasurementUnit ;
			owl:hasValue oe2022-iem:KilowattHour ;
		]
		;
	rdfs:label "energy usage" ;
	skos:definition "quantity of energy used to power some equipment, per unit time" ;
	.

oe2022-iem:Environment
	a owl:Class ;
	rdfs:subClassOf
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasAttribute ;
			owl:onClass oe2022-iem:AirQualityAttribute ;
			owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasAttribute ;
			owl:onClass oe2022-iem:AirSpeedAttribute ;
			owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasAttribute ;
			owl:onClass oe2022-iem:AirTemperatureAttribute ;
			owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasAttribute ;
			owl:onClass oe2022-iem:DaylightIntensityAttribute ;
			owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasAttribute ;
			owl:onClass oe2022-iem:RelativeHumidityAttribute ;
			owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		]
		;
	.

oe2022-iem:EnvironmentAttribute
	a owl:Class ;
	rdfs:subClassOf oe2022-iem:Measurement ;
	rdfs:label "environment attribute" ;
	skos:definition "a quantitative attribute of local environment" ;
	.

oe2022-iem:EnvironmentAttributeDelta
	a owl:Class ;
	rdfs:subClassOf [
		a owl:Restriction ;
		owl:onProperty oe2022-iem:hasSign ;
		owl:onClass oe2022-iem:Sign ;
		owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
	] ;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			oe2022-iem:DeltaCapableMeasurement
			oe2022-iem:EnvironmentAttribute
		) ;
	] ;
	.

oe2022-iem:Equipment
	a owl:Class ;
	rdfs:subClassOf
		oe2022-iem:RoomComponent ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasState ;
			owl:onClass oe2022-iem:ComponentState ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasAttribute ;
			owl:onClass oe2022-iem:EnergyUsage ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "equipment" ;
	owl:disjointWith oe2022-iem:Furniture ;
	skos:definition "room component that consumes energy" ;
	.

oe2022-iem:Fan
	a owl:Class ;
	rdfs:subClassOf
		oe2022-iem:Equipment ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasState ;
			owl:allValuesFrom [
				a owl:Class ;
				owl:unionOf (
					oe2022-iem:OffState
					oe2022-iem:OnState
				) ;
			] ;
		]
		;
	rdfs:label "fan" ;
	skos:definition "HVAC equipment that can increase air speed" ;
	.

oe2022-iem:Furniture
	a owl:Class ;
	rdfs:subClassOf
		oe2022-iem:RoomComponent ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasState ;
			owl:allValuesFrom [
				a owl:Class ;
				owl:unionOf (
					oe2022-iem:ClosedState
					oe2022-iem:OpenState
				) ;
			] ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasState ;
			owl:onClass oe2022-iem:ComponentState ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "furniture" ;
	skos:definition "room component that does not consume energy" ;
	.

oe2022-iem:High
	a
		owl:NamedIndividual ,
		oe2022-iem:Bucket
		;
	.

oe2022-iem:IndoorEnvironment
	a owl:Class ;
	rdfs:subClassOf
		oe2022-iem:Environment ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasAttribute ;
			owl:onClass oe2022-iem:DaylightIntensityAttribute ;
			owl:qualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		]
		;
	owl:disjointWith oe2022-iem:OutdoorEnvironment ;
	.

oe2022-iem:IndoorEnvironmentalQuality
	a owl:Class ;
	rdfs:label "indoor environmental quality" ;
	skos:definition "a perceived indoor experience of the building indoor including thermal comfort, indoor air quality, lighting, acoustics, and control systems" ;
	.

oe2022-iem:Kelvin
	a
		owl:NamedIndividual ,
		oe2022-iem:MeasurementUnit
		;
	rdfs:label "kelvin" ;
	dct:source "https://ucum.org/ucum.html#section-Base-Units" ;
	rdfs:seeAlso <https://www.nist.gov/pml/owm/si-units-temperature> ;
	skos:definition "base unit of temperature that is defined by taking the fixed numerical value of the Boltzmann constant k to be 1.380 649 ×10^−23 when expressed in the unit J K^−1, which is equal to kg m^2 s^−2 K^−1" ;
	cmns-av:explanatoryNote "The temperature 0 K is commonly referred to as 'absolute zero.' On the widely used Celsius temperature scale, water freezes at 0 °C and boils at about 100 °C. One Celsius degree is an interval of 1 K, and zero degrees Celsius is 273.15 K. An interval of one Celsius degree corresponds to an interval of 1.8 Fahrenheit degrees on the Fahrenheit temperature scale." ;
	cmns-av:symbol "K" ;
	.

oe2022-iem:Kilogram
	a
		owl:NamedIndividual ,
		oe2022-iem:MeasurementUnit
		;
	rdfs:label "kilogram" ;
	dct:source "https://ucum.org/ucum.html#section-Base-Units" ;
	rdfs:seeAlso <https://www.nist.gov/si-redefinition/kilogram> ;
	skos:definition "conversion-based unit of mass" ;
	skos:note "Note that in the International System of Units (SI), the kilogram is considered the base unit rather than the gram, which is the base unit in the UCUM." ;
	cmns-av:symbol "kg" ;
	.

oe2022-iem:KilowattHour
	a
		owl:NamedIndividual ,
		oe2022-iem:MeasurementUnit
		;
	rdfs:label "kilowatt-hour" ;
	skos:definition "measure of electrical energy equivalent to a power consumption of 1,000 watts for 1 hour" ;
	.

oe2022-iem:Low
	a
		owl:NamedIndividual ,
		oe2022-iem:Bucket
		;
	.

oe2022-iem:Measurement
	a owl:Class ;
	rdfs:subClassOf
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasBucket ;
			owl:onClass oe2022-iem:Bucket ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasMeasurementUnit ;
			owl:someValuesFrom oe2022-iem:MeasurementUnit ;
		]
		;
	rdfs:label "measurement" ;
	skos:definition "amount of something" ;
	.

oe2022-iem:MeasurementUnit
	a owl:Class ;
	rdfs:label "measurement unit" ;
	skos:definition "real scalar quantity, defined and adopted by convention, with which any other quantity of the same kind can be compared to express the ratio of the second quantity to the first one as a number" ;
	.

oe2022-iem:Medium
	a
		owl:NamedIndividual ,
		oe2022-iem:Bucket
		;
	.

oe2022-iem:MetersPerSecond
	a
		owl:NamedIndividual ,
		oe2022-iem:MeasurementUnit
		;
	rdfs:label "meter per second" ;
	skos:definition "measure of speed equipvalent to the moved meters for 1 second" ;
	.

oe2022-iem:Negative
	a
		owl:NamedIndividual ,
		oe2022-iem:Sign
		;
	.

oe2022-iem:Occupant
	a owl:Class ;
	rdfs:subClassOf
		cmns-pts:PartyRole ,
		[
			a owl:Restriction ;
			owl:onProperty cmns-pts:isPlayedBy ;
			owl:onClass oe2022-iem:Person ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "occupant"@en ;
	skos:definition "someone that resides or is present in or using a building, vehicle, seat, place, etc., at a given time"@en ;
	.

oe2022-iem:OffState
	a owl:Class ;
	rdfs:subClassOf oe2022-iem:ComponentState ;
	rdfs:label "off state" ;
	owl:disjointWith oe2022-iem:OnState ;
	skos:definition "condition of not being activated" ;
	.

oe2022-iem:OnState
	a owl:Class ;
	rdfs:subClassOf oe2022-iem:ComponentState ;
	rdfs:label "on state" ;
	skos:definition "condition of being currently in operation" ;
	.

oe2022-iem:OpenState
	a owl:Class ;
	rdfs:subClassOf oe2022-iem:ComponentState ;
	rdfs:label "open state" ;
	skos:definition "condition of being not closed" ;
	.

oe2022-iem:Organization
	a owl:Class ;
	rdfs:subClassOf cmns-pts:Party ;
	rdfs:label "organization"@en ;
	rdfs:seeAlso <https://www.w3.org/TR/vocab-org/#org:Organization> ;
	skos:definition "collection of one or more people, or groups of people formed together into a community or other social, commercial or political structure to act, or that is designated to act, towards some purpose, such as to meet a need or pursue collective goals on a continuing basis"@en ;
	skos:example "This may be a business entity, government, international organization, not-for-profit, academic institution, or other unincorporated and/or informal social organization."@en ;
	cmns-av:adaptedFrom "https://www.iso.org/obp/ui/#iso:std:iso-iec:6523:-1:ed-1:v1:en" ;
	.

oe2022-iem:OutdoorEnvironment
	a owl:Class ;
	rdfs:subClassOf
		oe2022-iem:DeltaDefinedEnvironment ,
		oe2022-iem:Environment
		;
	.

oe2022-iem:PerCubicSecond
	a owl:NamedIndividual ;
	.

oe2022-iem:PerCubicTime
	a owl:NamedIndividual ;
	.

oe2022-iem:PerHour
	a owl:NamedIndividual ;
	.

oe2022-iem:PerSecond
	a owl:NamedIndividual ;
	.

oe2022-iem:PerTime
	a owl:NamedIndividual ;
	.

oe2022-iem:Person
	a owl:Class ;
	rdfs:subClassOf cmns-pts:Party ;
	rdfs:label "person"@en ;
	skos:definition "individual human being, with consciousness of self"@en ;
	cmns-av:synonym "natural person"@en ;
	.

oe2022-iem:Positive
	a
		owl:NamedIndividual ,
		oe2022-iem:Sign
		;
	.

oe2022-iem:QuantityKind
	a owl:Class ;
	rdfs:label "quantity kind" ;
	skos:definition "aspect common to mutually comparable quantities" ;
	.

oe2022-iem:Radiator
	a owl:Class ;
	rdfs:subClassOf
		oe2022-iem:Equipment ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasState ;
			owl:allValuesFrom [
				a owl:Class ;
				owl:unionOf (
					oe2022-iem:OffState
					oe2022-iem:OnState
				) ;
			] ;
		]
		;
	rdfs:label "radiator" ;
	skos:definition "HVAC equipment that can raise the indoor temperature by transferring heat from an internal system into a room" ;
	.

oe2022-iem:RelativeHumidity
	a
		owl:NamedIndividual ,
		oe2022-iem:QuantityKind
		;
	rdfs:label "relative humidity" ;
	skos:definition "ratio of the mole fraction of water vapor to the mole fraction of water vapor saturated at the same temperature and barometric pressure." ;
	.

oe2022-iem:RelativeHumidityAttribute
	a owl:Class ;
	rdfs:subClassOf
		oe2022-iem:DeltaCapableMeasurement ,
		oe2022-iem:EnvironmentAttribute
		;
	.

oe2022-iem:ResultantIndoorEnvironment
	a owl:Class ;
	rdfs:subClassOf
		oe2022-iem:DeltaDefinedEnvironment ,
		oe2022-iem:IndoorEnvironment
		;
	.

oe2022-iem:Room
	a owl:Class ;
	rdfs:label "room" ;
	skos:definition "region in a building in which occupant comfort and measurements apply" ;
	.

oe2022-iem:RoomComponent
	a owl:Class ;
	rdfs:label "room component" ;
	skos:definition "an object in a room that can be used to alter relevant IEQ parameters" ;
	.

oe2022-iem:Second
	a owl:NamedIndividual ;
	.

oe2022-iem:Sign
	a owl:Class ;
	owl:equivalentClass [
		a owl:Class ;
		owl:oneOf (
			oe2022-iem:Negative
			oe2022-iem:Positive
		) ;
	] ;
	.

oe2022-iem:Speed
	a
		owl:NamedIndividual ,
		oe2022-iem:QuantityKind
		;
	.

oe2022-iem:SquareLength
	a owl:NamedIndividual ;
	.

oe2022-iem:SquareMeter
	a owl:NamedIndividual ;
	.

oe2022-iem:Temperature
	a
		owl:NamedIndividual ,
		oe2022-iem:QuantityKind
		;
	rdfs:label "temperature" ;
	dct:source "https://ucum.org/ucum.html#section-Base-Units" ;
	skos:definition "base quantity kind that is the quantitative measure of the ability of a substance, or more generally of any physical system, to transfer heat energy to another physical system" ;
	cmns-av:explanatoryNote "There are three types of temperature scale: those, such as the SI scale, that are defined in terms of the average translational kinetic energy per freely moving microscopic particle, such as atom, molecule, or electron, in a body; those that rely solely on purely macroscopic properties and thermodynamic principles, such as Kelvin's original definition; and those that are not defined by theoretical principles, but are defined by convenient empirical properties of particular materials." ;
	cmns-av:symbol "T" ;
	.

oe2022-iem:ThermalComfort
	a owl:Class ;
	rdfs:subClassOf oe2022-iem:IndoorEnvironmentalQuality ;
	rdfs:label "thermal comfort" ;
	skos:definition "the condition of mind which expresses satisfaction with the surrounding thermal environment and is assessed by subjective evaluation" ;
	.

oe2022-iem:Thermostat
	a owl:Class ;
	rdfs:subClassOf
		<heating,_ventilation,_and_air_conditioning> ,
		[
			a owl:Restriction ;
			owl:onProperty oe2022-iem:hasState ;
			owl:someValuesFrom oe2022-iem:ThermostatState ;
		]
		;
	.

oe2022-iem:ThermostatState
	a owl:Class ;
	rdfs:subClassOf oe2022-iem:ComponentState ;
	rdfs:label "thermostat state" ;
	skos:definition "numeric value at which the thermostat is set" ;
	.

oe2022-iem:Time
	a
		owl:NamedIndividual ,
		oe2022-iem:QuantityKind
		;
	.

oe2022-iem:Watt
	a owl:NamedIndividual ;
	rdfs:label "watt" ;
	skos:definition "unit of power equivalent to one joule per second" ;
	.

oe2022-iem:Window
	a owl:Class ;
	rdfs:subClassOf oe2022-iem:Furniture ;
	rdfs:label "window" ;
	owl:disjointWith oe2022-iem:WindowBlinds ;
	skos:definition "room component that can optionally let air flow between inside and outside regions" ;
	.

oe2022-iem:WindowBlinds
	a owl:Class ;
	rdfs:subClassOf oe2022-iem:Furniture ;
	rdfs:label "window blinds" ;
	skos:definition "room component that be open or closed, thereby letting in or blocking light, respectively" ;
	.

oe2022-iem:Work
	a owl:NamedIndividual ;
	.

oe2022-iem:hasAttribute
	a owl:ObjectProperty ;
	rdfs:subPropertyOf owl:topObjectProperty ;
	rdfs:label "has attribute" ;
	owl:inverseOf oe2022-iem:isAttributeOf ;
	skos:definition "has components that affect the nature of the whole with respect to relevant actors" ;
	.

oe2022-iem:hasBucket
	a owl:ObjectProperty ;
	rdfs:range oe2022-iem:Bucket ;
	.

oe2022-iem:hasEffect
	a owl:ObjectProperty ;
	rdfs:label "has effect" ;
	skos:definition "specify how to achange the parameters of indoor environmental quality" ;
	.

oe2022-iem:hasMeasurementUnit
	a owl:ObjectProperty ;
	rdfs:label "has measurement unit" ;
	rdfs:range oe2022-iem:MeasurementUnit ;
	skos:definition "indicates the unit in which something is expressed" ;
	.

oe2022-iem:hasNumericValue
	a owl:DatatypeProperty ;
	rdfs:label "has numeric value" ;
	rdfs:range xsd:int ;
	skos:definition "indicates a particular magnitude or designation for a given observable characteristic that is a number" ;
	.

oe2022-iem:hasPossibleState
	a owl:ObjectProperty ;
	rdfs:subPropertyOf owl:topObjectProperty ;
